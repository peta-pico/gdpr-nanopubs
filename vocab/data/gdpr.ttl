    eli:title_alternative "Article93(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article93 ;
    GDPRtext:isPartOfChapter gdpr:chapterX .

gdpr:article93-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
