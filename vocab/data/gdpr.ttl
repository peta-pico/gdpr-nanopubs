    GDPRtext:isPartOfPoint gdpr:article58-3 ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article58-3-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "to issue, on its own initiative or on request, opinions to the national parliament, the Member State government or, in accordance with Member State law, to other institutions and bodies as well as to the public on any issue related to the protection of personal data;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article58,
