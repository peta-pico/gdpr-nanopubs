    GDPRtext:isPartOfSection gdpr:chapterIII-2 .

gdpr:article13-2-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "where the processing is based on point (a) of Article 6(1) or point (a) of Article 9(2), the existence of the right to withdraw consent at any time, without affecting the lawfulness of processing based on consent before its withdrawal;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article13,
        gdpr:article13-2,
        gdpr:chapterIII,
        gdpr:chapterIII-2 ;
    eli:number "c"^^xsd:string ;
