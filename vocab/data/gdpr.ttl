gdpr:article24-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Taking into account the nature, scope, context and purposes of processing as well as the risks of varying likelihood and severity for the rights and freedoms of natural persons, the controller shall implement appropriate technical and organisational measures to ensure and to be able to demonstrate that processing is performed in accordance with this Regulation. Those measures shall be reviewed and updated where necessary."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article24,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article24(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article24 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article24-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where proportionate in relation to processing activities, the measures referred to in paragraph 1 shall include the implementation of appropriate data protection policies by the controller."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article24,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article24(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article24 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article24-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Adherence to approved codes of conduct as referred to in Article 40 or approved certification mechanisms as referred to in Article 42 may be used as an element by which to demonstrate compliance with the obligations of the controller."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article24,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article24(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article24 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article25-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Taking into account the state of the art, the cost of implementation and the nature, scope, context and purposes of processing as well as the risks of varying likelihood and severity for rights and freedoms of natural persons posed by the processing, the controller shall, both at the time of the determination of the means for processing and at the time of the processing itself, implement appropriate technical and organisational measures, such as pseudonymisation, which are designed to implement data-protection principles, such as data minimisation, in an effective manner and to integrate the necessary safeguards into the processing in order to meet the requirements of this Regulation and protect the rights of data subjects."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article25,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article25(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article25 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article25-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller shall implement appropriate technical and organisational measures for ensuring that, by default, only personal data which are necessary for each specific purpose of the processing are processed. That obligation applies to the amount of personal data collected, the extent of their processing, the period of their storage and their accessibility. In particular, such measures shall ensure that by default personal data are not made accessible without the individual's intervention to an indefinite number of natural persons."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article25,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article25(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article25 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article25-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "An approved certification mechanism pursuant to Article 42 may be used as an element to demonstrate compliance with the requirements set out in paragraphs 1 and 2 of this Article."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article25,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article25(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article25 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article26-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where two or more controllers jointly determine the purposes and means of processing, they shall be joint controllers. They shall in a transparent manner determine their respective responsibilities for compliance with the obligations under this Regulation, in particular as regards the exercising of the rights of the data subject and their respective duties to provide the information referred to in Articles 13 and 14, by means of an arrangement between them unless, and in so far as, the respective responsibilities of the controllers are determined by Union or Member State law to which the controllers are subject. The arrangement may designate a contact point for data subjects."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article26,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article26(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article26 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article26-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The arrangement referred to in paragraph 1 shall duly reflect the respective roles and relationships of the joint controllers vis-à-vis the data subjects. The essence of the arrangement shall be made available to the data subject."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article26,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article26(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article26 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article26-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Irrespective of the terms of the arrangement referred to in paragraph 1, the data subject may exercise his or her rights under this Regulation in respect of and against each of the controllers."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article26,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article26(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article26 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article27-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where Article 3(2) applies, the controller or the processor shall designate in writing a representative in the Union."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article27,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article27(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article27 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article27-2-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing which is occasional, does not include, on a large scale, processing of special categories of data as referred to in Article 9(1) or processing of personal data relating to criminal convictions and offences referred to in Article 10, and is unlikely to result in a risk to the rights and freedoms of natural persons, taking into account the nature, context, scope and purposes of the processing; or"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article27,
        gdpr:article27-2,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article27(2)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article27 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article27-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article27-2-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "a public authority or body."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article27,
        gdpr:article27-2,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article27(2)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article27 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article27-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article27-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The representative shall be established in one of the Member States where the data subjects, whose personal data are processed in relation to the offering of goods or services to them, or whose behaviour is monitored, are."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article27,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article27(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article27 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article27-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The representative shall be mandated by the controller or processor to be addressed in addition to or instead of the controller or the processor by, in particular, supervisory authorities and data subjects, on all issues related to processing, for the purposes of ensuring compliance with this Regulation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article27,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article27(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article27 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article27-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The designation of a representative by the controller or processor shall be without prejudice to legal actions which could be initiated against the controller or the processor themselves."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article27,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article27(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article27 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where processing is to be carried out on behalf of a controller, the controller shall use only processors providing sufficient guarantees to implement appropriate technical and organisational measures in such a manner that processing will meet the requirements of this Regulation and ensure the protection of the rights of the data subject."^^xsd:string,
        "With regard to point (h) of the first subparagraph, the processor shall immediately inform the controller if, in its opinion, an instruction infringes this Regulation or other Union or Member State data protection provisions."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article28(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-10 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Without prejudice to Articles 82, 83 and 84, if a processor infringes this Regulation by determining the purposes and means of processing, the processor shall be considered to be a controller in respect of that processing."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "10"^^xsd:string ;
    eli:title_alternative "Article28(10)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The processor shall not engage another processor without prior specific or general written authorisation of the controller. In the case of general written authorisation, the processor shall inform the controller of any intended changes concerning the addition or replacement of other processors, thereby giving the controller the opportunity to object to such changes."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article28(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-3-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processes the personal data only on documented instructions from the controller, including with regard to transfers of personal data to a third country or an international organisation, unless required to do so by Union or Member State law to which the processor is subject; in such a case, the processor shall inform the controller of that legal requirement before processing, unless that law prohibits such information on important grounds of public interest;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:article28-3,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article28(3)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article28-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-3-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "ensures that persons authorised to process the personal data have committed themselves to confidentiality or are under an appropriate statutory obligation of confidentiality;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:article28-3,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article28(3)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article28-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-3-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "takes all measures required pursuant to Article 32;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:article28-3,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article28(3)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article28-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-3-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "respects the conditions referred to in paragraphs 2 and 4 for engaging another processor;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:article28-3,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article28(3)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article28-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-3-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "taking into account the nature of the processing, assists the controller by appropriate technical and organisational measures, insofar as this is possible, for the fulfilment of the controller's obligation to respond to requests for exercising the data subject's rights laid down in Chapter III;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:article28-3,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article28(3)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article28-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-3-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "assists the controller in ensuring compliance with the obligations pursuant to Articles 32 to 36 taking into account the nature of processing and the information available to the processor;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:article28-3,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article28(3)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article28-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-3-g a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "at the choice of the controller, deletes or returns all the personal data to the controller after the end of the provision of services relating to processing, and deletes existing copies unless Union or Member State law requires storage of the personal data;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:article28-3,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "g"^^xsd:string ;
    eli:title_alternative "Article28(3)(g)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article28-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-3-h a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "makes available to the controller all information necessary to demonstrate compliance with the obligations laid down in this Article and allow for and contribute to audits, including inspections, conducted by the controller or another auditor mandated by the controller."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:article28-3,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "h"^^xsd:string ;
    eli:title_alternative "Article28(3)(h)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article28-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where a processor engages another processor for carrying out specific processing activities on behalf of the controller, the same data protection obligations as set out in the contract or other legal act between the controller and the processor as referred to in paragraph 3 shall be imposed on that other processor by way of a contract or other legal act under Union or Member State law, in particular providing sufficient guarantees to implement appropriate technical and organisational measures in such a manner that the processing will meet the requirements of this Regulation. Where that other processor fails to fulfil its data protection obligations, the initial processor shall remain fully liable to the controller for the performance of that other processor's obligations."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article28(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Adherence of a processor to an approved code of conduct as referred to in Article 40 or an approved certification mechanism as referred to in Article 42 may be used as an element by which to demonstrate sufficient guarantees as referred to in paragraphs 1 and 4 of this Article."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article28(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Without prejudice to an individual contract between the controller and the processor, the contract or the other legal act referred to in paragraphs 3 and 4 of this Article may be based, in whole or in part, on standard contractual clauses referred to in paragraphs 7 and 8 of this Article, including when they are part of a certification granted to the controller or processor pursuant to Articles 42 and 43."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article28(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-7 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Commission may lay down standard contractual clauses for the matters referred to in paragraph 3 and 4 of this Article and in accordance with the examination procedure referred to in Article 93(2)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "7"^^xsd:string ;
    eli:title_alternative "Article28(7)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-8 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "A supervisory authority may adopt standard contractual clauses for the matters referred to in paragraph 3 and 4 of this Article and in accordance with the consistency mechanism referred to in Article 63."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "8"^^xsd:string ;
    eli:title_alternative "Article28(8)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article28-9 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The contract or the other legal act referred to in paragraphs 3 and 4 shall be in writing, including in electronic form."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article28,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "9"^^xsd:string ;
    eli:title_alternative "Article28(9)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article28 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article29-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The processor and any person acting under the authority of the controller or of the processor, who has access to personal data, shall not process those data except on instructions from the controller, unless required to do so by Union or Member State law."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article29,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article29(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article29 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article3-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "This Regulation applies to the processing of personal data in the context of the activities of an establishment of a controller or a processor in the Union, regardless of whether the processing takes place in the Union or not."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article3,
        gdpr:chapterI ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article3(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article3 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article3-2-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the offering of goods or services, irrespective of whether a payment of the data subject is required, to such data subjects in the Union; or"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article3,
        gdpr:article3-2,
        gdpr:chapterI ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article3(2)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article3 ;
    GDPRtext:isPartOfChapter gdpr:chapterI ;
    GDPRtext:isPartOfPoint gdpr:article3-2 .

gdpr:article3-2-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the monitoring of their behaviour as far as their behaviour takes place within the Union."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article3,
        gdpr:article3-2,
        gdpr:chapterI ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article3(2)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article3 ;
    GDPRtext:isPartOfChapter gdpr:chapterI ;
    GDPRtext:isPartOfPoint gdpr:article3-2 .

gdpr:article3-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "This Regulation applies to the processing of personal data by a controller not established in the Union, but in a place where Member State law applies by virtue of public international law."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article3,
        gdpr:chapterI ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article3(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article3 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article30-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the name and contact details of the controller and, where applicable, the joint controller, the controller's representative and the data protection officer;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article30,
        gdpr:article30-1,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article30(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article30 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article30-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article30-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the purposes of the processing;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article30,
        gdpr:article30-1,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "b"^^xsd:string ;
