    GDPRtext:isPartOfPoint gdpr:article13-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-2 .

gdpr:article13-2-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the existence of automated decision-making, including profiling, referred to in Article 22(1) and (4) and, at least in those cases, meaningful information about the logic involved, as well as the significance and the envisaged consequences of such processing for the data subject."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article13,
        gdpr:article13-2,
        gdpr:chapterIII,
        gdpr:chapterIII-2 ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article13(2)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article13 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article13-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-2 .

gdpr:article13-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where the controller intends to further process the personal data for a purpose other than that for which the personal data were collected, the controller shall provide the data subject prior to that further processing with information on that other purpose and with any relevant further information as referred to in paragraph 2."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article13,
        gdpr:chapterIII,
        gdpr:chapterIII-2 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article13(3)"^^xsd:string ;
