    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article17-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where the controller has made the personal data public and is obliged pursuant to paragraph 1 to erase the personal data, the controller, taking account of available technology and the cost of implementation, shall take reasonable steps, including technical measures, to inform controllers which are processing the personal data that the data subject has requested the erasure by such controllers of any links to, or copy or replication of, those personal data."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article17,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article17(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article17 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article17-3-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "for exercising the right of freedom of expression and information;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article17,
        gdpr:article17-3,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article17(3)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article17 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article17-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article17-3-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "for compliance with a legal obligation which requires processing by Union or Member State law to which the controller is subject or for the performance of a task carried out in the public interest or in the exercise of official authority vested in the controller;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article17,
        gdpr:article17-3,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article17(3)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article17 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article17-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article17-3-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "for reasons of public interest in the area of public health in accordance with points (h) and (i) of Article 9(2) as well as Article 9(3);"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article17,
        gdpr:article17-3,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article17(3)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article17 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article17-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article17-3-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "for archiving purposes in the public interest, scientific or historical research purposes or statistical purposes in accordance with Article 89(1) in so far as the right referred to in paragraph 1 is likely to render impossible or seriously impair the achievement of the objectives of that processing; or"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article17,
        gdpr:article17-3,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article17(3)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article17 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article17-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article17-3-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "for the establishment, exercise or defence of legal claims."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article17,
        gdpr:article17-3,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article17(3)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article17 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article17-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article18-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the accuracy of the personal data is contested by the data subject, for a period enabling the controller to verify the accuracy of the personal data;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article18,
        gdpr:article18-1,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article18(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article18 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article18-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article18-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the processing is unlawful and the data subject opposes the erasure of the personal data and requests the restriction of their use instead;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article18,
        gdpr:article18-1,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article18(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article18 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article18-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article18-1-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the controller no longer needs the personal data for the purposes of the processing, but they are required by the data subject for the establishment, exercise or defence of legal claims;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article18,
        gdpr:article18-1,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article18(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article18 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article18-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article18-1-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the data subject has objected to processing pursuant to Article 21(1) pending the verification whether the legitimate grounds of the controller override those of the data subject."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article18,
        gdpr:article18-1,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article18(1)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article18 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article18-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article18-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where processing has been restricted under paragraph 1, such personal data shall, with the exception of storage, only be processed with the data subject's consent or for the establishment, exercise or defence of legal claims or for the protection of the rights of another natural or legal person or for reasons of important public interest of the Union or of a Member State."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article18,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article18(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article18 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article18-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "A data subject who has obtained restriction of processing pursuant to paragraph 1 shall be informed by the controller before the restriction of processing is lifted."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article18,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article18(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article18 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article19-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller shall communicate any rectification or erasure of personal data or restriction of processing carried out in accordance with Article 16, Article 17(1) and Article 18 to each recipient to whom the personal data have been disclosed, unless this proves impossible or involves disproportionate effort. The controller shall inform the data subject about those recipients if the data subject requests it."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article19,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article19(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article19 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article2-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "This Regulation applies to the processing of personal data wholly or partly by automated means and to the processing other than by automated means of personal data which form part of a filing system or are intended to form part of a filing system."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article2,
        gdpr:chapterI ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article2(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article2 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article2-2-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "in the course of an activity which falls outside the scope of Union law;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article2,
        gdpr:article2-2,
        gdpr:chapterI ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article2(2)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article2 ;
    GDPRtext:isPartOfChapter gdpr:chapterI ;
    GDPRtext:isPartOfPoint gdpr:article2-2 .

gdpr:article2-2-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "by the Member States when carrying out activities which fall within the scope of Chapter 2 of Title V of the TEU;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article2,
        gdpr:article2-2,
        gdpr:chapterI ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article2(2)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article2 ;
    GDPRtext:isPartOfChapter gdpr:chapterI ;
    GDPRtext:isPartOfPoint gdpr:article2-2 .

gdpr:article2-2-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "by a natural person in the course of a purely personal or household activity;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article2,
        gdpr:article2-2,
        gdpr:chapterI ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article2(2)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article2 ;
    GDPRtext:isPartOfChapter gdpr:chapterI ;
    GDPRtext:isPartOfPoint gdpr:article2-2 .

gdpr:article2-2-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "by competent authorities for the purposes of the prevention, investigation, detection or prosecution of criminal offences or the execution of criminal penalties, including the safeguarding against and the prevention of threats to public security."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article2,
        gdpr:article2-2,
        gdpr:chapterI ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article2(2)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article2 ;
    GDPRtext:isPartOfChapter gdpr:chapterI ;
    GDPRtext:isPartOfPoint gdpr:article2-2 .

gdpr:article2-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "For the processing of personal data by the Union institutions, bodies, offices and agencies, Regulation (EC) No 45/2001 applies. Regulation (EC) No 45/2001 and other Union legal acts applicable to such processing of personal data shall be adapted to the principles and rules of this Regulation in accordance with Article 98."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article2,
        gdpr:chapterI ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article2(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article2 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article2-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "This Regulation shall be without prejudice to the application of Directive 2000/31/EC, in particular of the liability rules of intermediary service providers in Articles 12 to 15 of that Directive."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article2,
        gdpr:chapterI ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article2(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article2 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article20-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the processing is based on consent pursuant to point (a) of Article 6(1) or point (a) of Article 9(2) or on a contract pursuant to point (b) of Article 6(1); and"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article20,
        gdpr:article20-1,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article20(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article20 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article20-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article20-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the processing is carried out by automated means."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article20,
        gdpr:article20-1,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article20(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article20 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article20-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article20-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "In exercising his or her right to data portability pursuant to paragraph 1, the data subject shall have the right to have the personal data transmitted directly from one controller to another, where technically feasible."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article20,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article20(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article20 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article20-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The exercise of the right referred to in paragraph 1 of this Article shall be without prejudice to Article 17. That right shall not apply to processing necessary for the performance of a task carried out in the public interest or in the exercise of official authority vested in the controller."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article20,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article20(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article20 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article20-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The right referred to in paragraph 1 shall not adversely affect the rights and freedoms of others."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article20,
        gdpr:chapterIII,
        gdpr:chapterIII-3 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article20(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article20 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfSection gdpr:chapterIII-3 .

gdpr:article21-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The data subject shall have the right to object, on grounds relating to his or her particular situation, at any time to processing of personal data concerning him or her which is based on point (e) or (f) of Article 6(1), including profiling based on those provisions. The controller shall no longer process the personal data unless the controller demonstrates compelling legitimate grounds for the processing which override the interests, rights and freedoms of the data subject or for the establishment, exercise or defence of legal claims."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article21,
        gdpr:chapterIII,
        gdpr:chapterIII-4 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article21(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article21 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfSection gdpr:chapterIII-4 .

gdpr:article21-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where personal data are processed for direct marketing purposes, the data subject shall have the right to object at any time to processing of personal data concerning him or her for such marketing, which includes profiling to the extent that it is related to such direct marketing."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article21,
        gdpr:chapterIII,
        gdpr:chapterIII-4 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article21(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article21 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfSection gdpr:chapterIII-4 .

gdpr:article21-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where the data subject objects to processing for direct marketing purposes, the personal data shall no longer be processed for such purposes."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article21,
        gdpr:chapterIII,
        gdpr:chapterIII-4 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article21(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article21 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfSection gdpr:chapterIII-4 .

gdpr:article21-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "At the latest at the time of the first communication with the data subject, the right referred to in paragraphs 1 and 2 shall be explicitly brought to the attention of the data subject and shall be presented clearly and separately from any other information."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article21,
        gdpr:chapterIII,
        gdpr:chapterIII-4 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article21(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article21 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfSection gdpr:chapterIII-4 .

gdpr:article21-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "In the context of the use of information society services, and notwithstanding Directive 2002/58/EC, the data subject may exercise his or her right to object by automated means using technical specifications."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article21,
        gdpr:chapterIII,
        gdpr:chapterIII-4 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article21(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article21 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfSection gdpr:chapterIII-4 .

gdpr:article21-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where personal data are processed for scientific or historical research purposes or statistical purposes pursuant to Article 89(1), the data subject, on grounds relating to his or her particular situation, shall have the right to object to processing of personal data concerning him or her, unless the processing is necessary for the performance of a task carried out for reasons of public interest."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article21,
        gdpr:chapterIII,
        gdpr:chapterIII-4 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article21(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article21 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfSection gdpr:chapterIII-4 .

gdpr:article22-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The data subject shall have the right not to be subject to a decision based solely on automated processing, including profiling, which produces legal effects concerning him or her or similarly significantly affects him or her."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article22,
        gdpr:chapterIII,
        gdpr:chapterIII-4 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article22(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article22 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfSection gdpr:chapterIII-4 .

gdpr:article22-2-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "is necessary for entering into, or performance of, a contract between the data subject and a data controller;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article22,
        gdpr:article22-2,
        gdpr:chapterIII,
        gdpr:chapterIII-4 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article22(2)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article22 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article22-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-4 .

gdpr:article22-2-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "is authorised by Union or Member State law to which the controller is subject and which also lays down suitable measures to safeguard the data subject's rights and freedoms and legitimate interests; or"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article22,
        gdpr:article22-2,
        gdpr:chapterIII,
        gdpr:chapterIII-4 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article22(2)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article22 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article22-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-4 .

gdpr:article22-2-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "is based on the data subject's explicit consent."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article22,
        gdpr:article22-2,
        gdpr:chapterIII,
        gdpr:chapterIII-4 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article22(2)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article22 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article22-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-4 .

gdpr:article22-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "In the cases referred to in points (a) and (c) of paragraph 2, the data controller shall implement suitable measures to safeguard the data subject's rights and freedoms and legitimate interests, at least the right to obtain human intervention on the part of the controller, to express his or her point of view and to contest the decision."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article22,
        gdpr:chapterIII,
        gdpr:chapterIII-4 ;
    eli:number "3"^^xsd:string ;
