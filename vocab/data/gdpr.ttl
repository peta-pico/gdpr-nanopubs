    GDPRtext:isPartOfArticle gdpr:article93 ;
    GDPRtext:isPartOfChapter gdpr:chapterX .

gdpr:article93-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where reference is made to this paragraph, Article 8 of Regulation (EU) No 182/2011, in conjunction with Article 5 thereof, shall apply."^^xsd:string ;
