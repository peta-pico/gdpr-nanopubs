gdpr:article58-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The exercise of the powers conferred on the supervisory authority pursuant to this Article shall be subject to appropriate safeguards, including effective judicial remedy and due process, set out in Union and Member State law in accordance with the Charter."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article58,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article58(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article58 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article58-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each Member State shall provide by law that its supervisory authority shall have the power to bring infringements of this Regulation to the attention of the judicial authorities and where appropriate, to commence or engage otherwise in legal proceedings, in order to enforce the provisions of this Regulation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article58,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article58(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article58 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article58-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each Member State may provide by law that its supervisory authority shall have additional powers to those referred to in paragraphs 1, 2 and 3. The exercise of those powers shall not impair the effective operation of Chapter VII."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article58,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article58(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article58 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article59-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each supervisory authority shall draw up an annual report on its activities, which may include a list of types of infringement notified and types of measures taken in accordance with Article 58(2). Those reports shall be transmitted to the national parliament, the government and other authorities as designated by Member State law. They shall be made available to the public, to the Commission and to the Board."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article59,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article59(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article59 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article6-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the data subject has given consent to the processing of his or her personal data for one or more specific purposes;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article6,
        gdpr:article6-1,
        gdpr:chapterII ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article6(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article6 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article6-1 .

gdpr:article6-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing is necessary for the performance of a contract to which the data subject is party or in order to take steps at the request of the data subject prior to entering into a contract;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article6,
        gdpr:article6-1,
        gdpr:chapterII ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article6(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article6 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article6-1 .

gdpr:article6-1-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing is necessary for compliance with a legal obligation to which the controller is subject;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article6,
        gdpr:article6-1,
        gdpr:chapterII ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article6(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article6 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article6-1 .

gdpr:article6-1-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing is necessary in order to protect the vital interests of the data subject or of another natural person;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article6,
        gdpr:article6-1,
        gdpr:chapterII ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article6(1)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article6 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article6-1 .

gdpr:article6-1-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing is necessary for the performance of a task carried out in the public interest or in the exercise of official authority vested in the controller;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article6,
        gdpr:article6-1,
        gdpr:chapterII ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article6(1)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article6 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article6-1 .

gdpr:article6-1-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing is necessary for the purposes of the legitimate interests pursued by the controller or by a third party, except where such interests are overridden by the interests or fundamental rights and freedoms of the data subject which require protection of personal data, in particular where the data subject is a child."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article6,
        gdpr:article6-1,
        gdpr:chapterII ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article6(1)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article6 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article6-1 .

gdpr:article6-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Member States may maintain or introduce more specific provisions to adapt the application of the rules of this Regulation with regard to processing for compliance with points (c) and (e) of paragraph 1 by determining more precisely specific requirements for the processing and other measures to ensure lawful and fair processing including for other specific processing situations as provided for in Chapter IX."^^xsd:string,
        "The purpose of the processing shall be determined in that legal basis or, as regards the processing referred to in point (e) of paragraph 1, shall be necessary for the performance of a task carried out in the public interest or in the exercise of official authority vested in the controller. That legal basis may contain specific provisions to adapt the application of rules of this Regulation, inter alia: the general conditions governing the lawfulness of processing by the controller; the types of data which are subject to the processing; the data subjects concerned; the entities to, and the purposes for which, the personal data may be disclosed; the purpose limitation; storage periods; and processing operations and processing procedures, including measures to ensure lawful and fair processing such as those for other specific processing situations as provided for in Chapter IX. The Union or the Member State law shall meet an objective of public interest and be proportionate to the legitimate aim pursued."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article6,
        gdpr:chapterII ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article6(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article6 ;
    GDPRtext:isPartOfChapter gdpr:chapterII .

gdpr:article6-3-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "Union law; or"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article6,
        gdpr:article6-3,
        gdpr:chapterII ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article6(3)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article6 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article6-3 .

gdpr:article6-3-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "Member State law to which the controller is subject."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article6,
        gdpr:article6-3,
        gdpr:chapterII ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article6(3)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article6 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article6-3 .

gdpr:article6-4-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "any link between the purposes for which the personal data have been collected and the purposes of the intended further processing;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article6,
        gdpr:article6-4,
        gdpr:chapterII ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article6(4)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article6 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article6-4 .

gdpr:article6-4-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the context in which the personal data have been collected, in particular regarding the relationship between data subjects and the controller;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article6,
        gdpr:article6-4,
        gdpr:chapterII ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article6(4)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article6 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article6-4 .

gdpr:article6-4-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the nature of the personal data, in particular whether special categories of personal data are processed, pursuant to Article 9, or whether personal data related to criminal convictions and offences are processed, pursuant to Article 10;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article6,
        gdpr:article6-4,
        gdpr:chapterII ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article6(4)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article6 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article6-4 .

gdpr:article6-4-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the possible consequences of the intended further processing for data subjects;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article6,
        gdpr:article6-4,
        gdpr:chapterII ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article6(4)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article6 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article6-4 .

gdpr:article6-4-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the existence of appropriate safeguards, which may include encryption or pseudonymisation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article6,
        gdpr:article6-4,
        gdpr:chapterII ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article6(4)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article6 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article6-4 .

gdpr:article60-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The lead supervisory authority shall cooperate with the other supervisory authorities concerned in accordance with this Article in an endeavour to reach consensus. The lead supervisory authority and the supervisory authorities concerned shall exchange all relevant information with each other."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article60,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article60(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article60 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article60-10 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "After being notified of the decision of the lead supervisory authority pursuant to paragraphs 7 and 9, the controller or processor shall take the necessary measures to ensure compliance with the decision as regards processing activities in the context of all its establishments in the Union. The controller or processor shall notify the measures taken for complying with the decision to the lead supervisory authority, which shall inform the other supervisory authorities concerned."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article60,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "10"^^xsd:string ;
    eli:title_alternative "Article60(10)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article60 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article60-11 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where, in exceptional circumstances, a supervisory authority concerned has reasons to consider that there is an urgent need to act in order to protect the interests of data subjects, the urgency procedure referred to in Article 66 shall apply."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article60,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "11"^^xsd:string ;
    eli:title_alternative "Article60(11)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article60 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article60-12 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The lead supervisory authority and the other supervisory authorities concerned shall supply the information required under this Article to each other by electronic means, using a standardised format."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article60,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "12"^^xsd:string ;
    eli:title_alternative "Article60(12)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article60 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article60-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The lead supervisory authority may request at any time other supervisory authorities concerned to provide mutual assistance pursuant to Article 61 and may conduct joint operations pursuant to Article 62, in particular for carrying out investigations or for monitoring the implementation of a measure concerning a controller or processor established in another Member State."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article60,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article60(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article60 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article60-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The lead supervisory authority shall, without delay, communicate the relevant information on the matter to the other supervisory authorities concerned. It shall without delay submit a draft decision to the other supervisory authorities concerned for their opinion and take due account of their views."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article60,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article60(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article60 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article60-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where any of the other supervisory authorities concerned within a period of four weeks after having been consulted in accordance with paragraph 3 of this Article, expresses a relevant and reasoned objection to the draft decision, the lead supervisory authority shall, if it does not follow the relevant and reasoned objection or is of the opinion that the objection is not relevant or reasoned, submit the matter to the consistency mechanism referred to in Article 63."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article60,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article60(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article60 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article60-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where the lead supervisory authority intends to follow the relevant and reasoned objection made, it shall submit to the other supervisory authorities concerned a revised draft decision for their opinion. That revised draft decision shall be subject to the procedure referred to in paragraph 4 within a period of two weeks."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article60,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article60(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article60 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article60-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where none of the other supervisory authorities concerned has objected to the draft decision submitted by the lead supervisory authority within the period referred to in paragraphs 4 and 5, the lead supervisory authority and the supervisory authorities concerned shall be deemed to be in agreement with that draft decision and shall be bound by it."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article60,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article60(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article60 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article60-7 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The lead supervisory authority shall adopt and notify the decision to the main establishment or single establishment of the controller or processor, as the case may be and inform the other supervisory authorities concerned and the Board of the decision in question, including a summary of the relevant facts and grounds. The supervisory authority with which a complaint has been lodged shall inform the complainant on the decision."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article60,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "7"^^xsd:string ;
    eli:title_alternative "Article60(7)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article60 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article60-8 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "By derogation from paragraph 7, where a complaint is dismissed or rejected, the supervisory authority with which the complaint was lodged shall adopt the decision and notify it to the complainant and shall inform the controller thereof."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article60,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "8"^^xsd:string ;
    eli:title_alternative "Article60(8)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article60 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article60-9 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where the lead supervisory authority and the supervisory authorities concerned agree to dismiss or reject parts of a complaint and to act on other parts of that complaint, a separate decision shall be adopted for each of those parts of the matter. The lead supervisory authority shall adopt the decision for the part concerning actions in relation to the controller, shall notify it to the main establishment or single establishment of the controller or processor on the territory of its Member State and shall inform the complainant thereof, while the supervisory authority of the complainant shall adopt the decision for the part concerning dismissal or rejection of that complaint, and shall notify it to that complainant and shall inform the controller or processor thereof."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article60,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "9"^^xsd:string ;
    eli:title_alternative "Article60(9)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article60 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article61-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Supervisory authorities shall provide each other with relevant information and mutual assistance in order to implement and apply this Regulation in a consistent manner, and shall put in place measures for effective cooperation with one another. Mutual assistance shall cover, in particular, information requests and supervisory measures, such as requests to carry out prior authorisations and consultations, inspections and investigations."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article61,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article61(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article61 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article61-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each supervisory authority shall take all appropriate measures required to reply to a request of another supervisory authority without undue delay and no later than one month after receiving the request. Such measures may include, in particular, the transmission of relevant information on the conduct of an investigation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article61,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article61(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article61 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article61-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Requests for assistance shall contain all the necessary information, including the purpose of and reasons for the request. Information exchanged shall be used only for the purpose for which it was requested."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article61,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article61(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article61 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article61-4-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "it is not competent for the subject-matter of the request or for the measures it is requested to execute; or"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article61,
        gdpr:article61-4,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article61(4)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article61 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article61-4 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article61-4-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "compliance with the request would infringe this Regulation or Union or Member State law to which the supervisory authority receiving the request is subject."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article61,
        gdpr:article61-4,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article61(4)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article61 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article61-4 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article61-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The requested supervisory authority shall inform the requesting supervisory authority of the results or, as the case may be, of the progress of the measures taken in order to respond to the request. The requested supervisory authority shall provide reasons for any refusal to comply with a request pursuant to paragraph 4."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article61,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article61(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article61 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article61-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Requested supervisory authorities shall, as a rule, supply the information requested by other supervisory authorities by electronic means, using a standardised format."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article61,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article61(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article61 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article61-7 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Requested supervisory authorities shall not charge a fee for any action taken by them pursuant to a request for mutual assistance. Supervisory authorities may agree on rules to indemnify each other for specific expenditure arising from the provision of mutual assistance in exceptional circumstances."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article61,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "7"^^xsd:string ;
    eli:title_alternative "Article61(7)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article61 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article61-8 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where a supervisory authority does not provide the information referred to in paragraph 5 of this Article within one month of receiving the request of another supervisory authority, the requesting supervisory authority may adopt a provisional measure on the territory of its Member State in accordance with Article 55(1). In that case, the urgent need to act under Article 66(1) shall be presumed to be met and require an urgent binding decision from the Board pursuant to Article 66(2)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article61,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "8"^^xsd:string ;
    eli:title_alternative "Article61(8)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article61 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article61-9 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Commission may, by means of implementing acts, specify the format and procedures for mutual assistance referred to in this Article and the arrangements for the exchange of information by electronic means between supervisory authorities, and between supervisory authorities and the Board, in particular the standardised format referred to in paragraph 6 of this Article. Those implementing acts shall be adopted in accordance with the examination procedure referred to in Article 93(2)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article61,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "9"^^xsd:string ;
    eli:title_alternative "Article61(9)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article61 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article62-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The supervisory authorities shall, where appropriate, conduct joint operations including joint investigations and joint enforcement measures in which members or staff of the supervisory authorities of other Member States are involved."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article62,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article62(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article62 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article62-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where the controller or processor has establishments in several Member States or where a significant number of data subjects in more than one Member State are likely to be substantially affected by processing operations, a supervisory authority of each of those Member States shall have the right to participate in joint operations. The supervisory authority which is competent pursuant to Article 56(1) or (4) shall invite the supervisory authority of each of those Member States to take part in the joint operations and shall respond without delay to the request of a supervisory authority to participate."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article62,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article62(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article62 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article62-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "A supervisory authority may, in accordance with Member State law, and with the seconding supervisory authority's authorisation, confer powers, including investigative powers on the seconding supervisory authority's members or staff involved in joint operations or, in so far as the law of the Member State of the host supervisory authority permits, allow the seconding supervisory authority's members or staff to exercise their investigative powers in accordance with the law of the Member State of the seconding supervisory authority. Such investigative powers may be exercised only under the guidance and in the presence of members or staff of the host supervisory authority. The seconding supervisory authority's members or staff shall be subject to the Member State law of the host supervisory authority."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article62,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article62(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article62 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article62-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where, in accordance with paragraph 1, staff of a seconding supervisory authority operate in another Member State, the Member State of the host supervisory authority shall assume responsibility for their actions, including liability, for any damage caused by them during their operations, in accordance with the law of the Member State in whose territory they are operating."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article62,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article62(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article62 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article62-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Member State in whose territory the damage was caused shall make good such damage under the conditions applicable to damage caused by its own staff. The Member State of the seconding supervisory authority whose staff has caused damage to any person in the territory of another Member State shall reimburse that other Member State in full any sums it has paid to the persons entitled on their behalf."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article62,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article62(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article62 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article62-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Without prejudice to the exercise of its rights vis-à-vis third parties and with the exception of paragraph 5, each Member State shall refrain, in the case provided for in paragraph 1, from requesting reimbursement from another Member State in relation to damage referred to in paragraph 4."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article62,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article62(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article62 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article62-7 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where a joint operation is intended and a supervisory authority does not, within one month, comply with the obligation laid down in the second sentence of paragraph 2 of this Article, the other supervisory authorities may adopt a provisional measure on the territory of its Member State in accordance with Article 55. In that case, the urgent need to act under Article 66(1) shall be presumed to be met and require an opinion or an urgent binding decision from the Board pursuant to Article 66(2)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article62,
        gdpr:chapterVII,
        gdpr:chapterVII-1 ;
    eli:number "7"^^xsd:string ;
    eli:title_alternative "Article62(7)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article62 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-1 .

gdpr:article63-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "In order to contribute to the consistent application of this Regulation throughout the Union, the supervisory authorities shall cooperate with each other and, where relevant, with the Commission, through the consistency mechanism as set out in this Section."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article63,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article63(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article63 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article64-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "aims to adopt a list of the processing operations subject to the requirement for a data protection impact assessment pursuant to Article 35(4);"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article64,
        gdpr:article64-1,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article64(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article64 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article64-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article64-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "concerns a matter pursuant to Article 40(7) whether a draft code of conduct or an amendment or extension to a code of conduct complies with this Regulation;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article64,
        gdpr:article64-1,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article64(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article64 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article64-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article64-1-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "aims to approve the criteria for accreditation of a body pursuant to Article 41(3) or a certification body pursuant to Article 43(3);"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article64,
        gdpr:article64-1,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article64(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article64 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article64-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article64-1-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "aims to determine standard data protection clauses referred to in point (d) of Article 46(2) and in Article 28(8);"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article64,
        gdpr:article64-1,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article64(1)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article64 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article64-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article64-1-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "aims to authorise contractual clauses referred to in point (a) of Article 46(3); or"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article64,
        gdpr:article64-1,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article64(1)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article64 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article64-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article64-1-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "aims to approve binding corporate rules within the meaning of Article 47."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article64,
        gdpr:article64-1,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article64(1)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article64 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article64-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article64-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Any supervisory authority, the Chair of the Board or the Commission may request that any matter of general application or producing effects in more than one Member State be examined by the Board with a view to obtaining an opinion, in particular where a competent supervisory authority does not comply with the obligations for mutual assistance in accordance with Article 61 or for joint operations in accordance with Article 62."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article64,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article64(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article64 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article64-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "In the cases referred to in paragraphs 1 and 2, the Board shall issue an opinion on the matter submitted to it provided that it has not already issued an opinion on the same matter. That opinion shall be adopted within eight weeks by simple majority of the members of the Board. That period may be extended by a further six weeks, taking into account the complexity of the subject matter. Regarding the draft decision referred to in paragraph 1 circulated to the members of the Board in accordance with paragraph 5, a member which has not objected within a reasonable period indicated by the Chair, shall be deemed to be in agreement with the draft decision."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article64,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article64(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article64 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article64-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Supervisory authorities and the Commission shall, without undue delay, communicate by electronic means to the Board, using a standardised format any relevant information, including as the case may be a summary of the facts, the draft decision, the grounds which make the enactment of such measure necessary, and the views of other supervisory authorities concerned."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article64,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article64(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article64 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article64-5-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the members of the Board and the Commission of any relevant information which has been communicated to it using a standardised format. The secretariat of the Board shall, where necessary, provide translations of relevant information; and"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article64,
        gdpr:article64-5,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article64(5)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article64 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article64-5 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article64-5-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the supervisory authority referred to, as the case may be, in paragraphs 1 and 2, and the Commission of the opinion and make it public."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article64,
        gdpr:article64-5,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article64(5)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article64 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article64-5 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article64-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The competent supervisory authority shall not adopt its draft decision referred to in paragraph 1 within the period referred to in paragraph 3."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article64,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article64(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article64 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article64-7 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The supervisory authority referred to in paragraph 1 shall take utmost account of the opinion of the Board and shall, within two weeks after receiving the opinion, communicate to the Chair of the Board by electronic means whether it will maintain or amend its draft decision and, if any, the amended draft decision, using a standardised format."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article64,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "7"^^xsd:string ;
    eli:title_alternative "Article64(7)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article64 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article64-8 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where the supervisory authority concerned informs the Chair of the Board within the period referred to in paragraph 7 of this Article that it does not intend to follow the opinion of the Board, in whole or in part, providing the relevant grounds, Article 65(1) shall apply."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article64,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "8"^^xsd:string ;
    eli:title_alternative "Article64(8)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article64 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article65-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "where, in a case referred to in Article 60(4), a supervisory authority concerned has raised a relevant and reasoned objection to a draft decision of the lead authority or the lead authority has rejected such an objection as being not relevant or reasoned. The binding decision shall concern all the matters which are the subject of the relevant and reasoned objection, in particular whether there is an infringement of this Regulation;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article65,
        gdpr:article65-1,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article65(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article65 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article65-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article65-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "where there are conflicting views on which of the supervisory authorities concerned is competent for the main establishment;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article65,
        gdpr:article65-1,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article65(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article65 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article65-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article65-1-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "where a competent supervisory authority does not request the opinion of the Board in the cases referred to in Article 64(1), or does not follow the opinion of the Board issued under Article 64. In that case, any supervisory authority concerned or the Commission may communicate the matter to the Board."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article65,
        gdpr:article65-1,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article65(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article65 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article65-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article65-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The decision referred to in paragraph 1 shall be adopted within one month from the referral of the subject-matter by a two-thirds majority of the members of the Board. That period may be extended by a further month on account of the complexity of the subject-matter. The decision referred to in paragraph 1 shall be reasoned and addressed to the lead supervisory authority and all the supervisory authorities concerned and binding on them."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article65,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article65(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article65 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article65-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where the Board has been unable to adopt a decision within the periods referred to in paragraph 2, it shall adopt its decision within two weeks following the expiration of the second month referred to in paragraph 2 by a simple majority of the members of the Board. Where the members of the Board are split, the decision shall by adopted by the vote of its Chair."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article65,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article65(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article65 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article65-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The supervisory authorities concerned shall not adopt a decision on the subject matter submitted to the Board under paragraph 1 during the periods referred to in paragraphs 2 and 3."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article65,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article65(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article65 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article65-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Chair of the Board shall notify, without undue delay, the decision referred to in paragraph 1 to the supervisory authorities concerned. It shall inform the Commission thereof. The decision shall be published on the website of the Board without delay after the supervisory authority has notified the final decision referred to in paragraph 6."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article65,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article65(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article65 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article65-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The lead supervisory authority or, as the case may be, the supervisory authority with which the complaint has been lodged shall adopt its final decision on the basis of the decision referred to in paragraph 1 of this Article, without undue delay and at the latest by one month after the Board has notified its decision. The lead supervisory authority or, as the case may be, the supervisory authority with which the complaint has been lodged, shall inform the Board of the date when its final decision is notified respectively to the controller or the processor and to the data subject. The final decision of the supervisory authorities concerned shall be adopted under the terms of Article 60(7), (8) and (9). The final decision shall refer to the decision referred to in paragraph 1 of this Article and shall specify that the decision referred to in that paragraph will be published on the website of the Board in accordance with paragraph 5 of this Article. The final decision shall attach the decision referred to in paragraph 1 of this Article."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article65,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article65(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article65 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article66-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "In exceptional circumstances, where a supervisory authority concerned considers that there is an urgent need to act in order to protect the rights and freedoms of data subjects, it may, by way of derogation from the consistency mechanism referred to in Articles 63, 64 and 65 or the procedure referred to in Article 60, immediately adopt provisional measures intended to produce legal effects on its own territory with a specified period of validity which shall not exceed three months. The supervisory authority shall, without delay, communicate those measures and the reasons for adopting them to the other supervisory authorities concerned, to the Board and to the Commission."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article66,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article66(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article66 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article66-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where a supervisory authority has taken a measure pursuant to paragraph 1 and considers that final measures need urgently be adopted, it may request an urgent opinion or an urgent binding decision from the Board, giving reasons for requesting such opinion or decision."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article66,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article66(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article66 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article66-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Any supervisory authority may request an urgent opinion or an urgent binding decision, as the case may be, from the Board where a competent supervisory authority has not taken an appropriate measure in a situation where there is an urgent need to act, in order to protect the rights and freedoms of data subjects, giving reasons for requesting such opinion or decision, including for the urgent need to act."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article66,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article66(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article66 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article66-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "By derogation from Article 64(3) and Article 65(2), an urgent opinion or an urgent binding decision referred to in paragraphs 2 and 3 of this Article shall be adopted within two weeks by simple majority of the members of the Board."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article66,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article66(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article66 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article67-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Commission may adopt implementing acts of general scope in order to specify the arrangements for the exchange of information by electronic means between supervisory authorities, and between supervisory authorities and the Board, in particular the standardised format referred to in Article 64."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article67,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article67(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article67 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article67-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Those implementing acts shall be adopted in accordance with the examination procedure referred to in Article 93(2)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article67,
        gdpr:chapterVII,
        gdpr:chapterVII-2 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article67(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article67 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-2 .

gdpr:article68-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The European Data Protection Board (the ‘Board’) is hereby established as a body of the Union and shall have legal personality."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article68,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article68(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article68 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article68-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Board shall be represented by its Chair."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article68,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article68(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article68 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article68-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Board shall be composed of the head of one supervisory authority of each Member State and of the European Data Protection Supervisor, or their respective representatives."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article68,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article68(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article68 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article68-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where in a Member State more than one supervisory authority is responsible for monitoring the application of the provisions pursuant to this Regulation, a joint representative shall be appointed in accordance with that Member State's law."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article68,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article68(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article68 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article68-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Commission shall have the right to participate in the activities and meetings of the Board without voting right. The Commission shall designate a representative. The Chair of the Board shall communicate to the Commission the activities of the Board."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article68,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article68(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article68 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article68-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "In the cases referred to in Article 65, the European Data Protection Supervisor shall have voting rights only on decisions which concern principles and rules applicable to the Union institutions, bodies, offices and agencies which correspond in substance to those of this Regulation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article68,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article68(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article68 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article69-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Board shall act independently when performing its tasks or exercising its powers pursuant to Articles 70 and 71."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article69,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article69(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article69 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article69-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Without prejudice to requests by the Commission referred to in point (b) of Article 70(1) and in Article 70(2), the Board shall, in the performance of its tasks or the exercise of its powers, neither seek nor take instructions from anybody."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article69,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article69(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article69 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article7-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where processing is based on consent, the controller shall be able to demonstrate that the data subject has consented to processing of his or her personal data."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article7,
        gdpr:chapterII ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article7(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article7 ;
    GDPRtext:isPartOfChapter gdpr:chapterII .

gdpr:article7-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "If the data subject's consent is given in the context of a written declaration which also concerns other matters, the request for consent shall be presented in a manner which is clearly distinguishable from the other matters, in an intelligible and easily accessible form, using clear and plain language. Any part of such a declaration which constitutes an infringement of this Regulation shall not be binding."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article7,
        gdpr:chapterII ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article7(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article7 ;
    GDPRtext:isPartOfChapter gdpr:chapterII .

gdpr:article7-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The data subject shall have the right to withdraw his or her consent at any time. The withdrawal of consent shall not affect the lawfulness of processing based on consent before its withdrawal. Prior to giving consent, the data subject shall be informed thereof. It shall be as easy to withdraw as to give consent."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article7,
        gdpr:chapterII ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article7(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article7 ;
    GDPRtext:isPartOfChapter gdpr:chapterII .

gdpr:article7-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "When assessing whether consent is freely given, utmost account shall be taken of whether, inter alia, the performance of a contract, including the provision of a service, is conditional on consent to the processing of personal data that is not necessary for the performance of that contract."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article7,
        gdpr:chapterII ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article7(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article7 ;
    GDPRtext:isPartOfChapter gdpr:chapterII .

gdpr:article70-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "monitor and ensure the correct application of this Regulation in the cases provided for in Articles 64 and 65 without prejudice to the tasks of national supervisory authorities;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article70,
        gdpr:article70-1,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article70(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article70 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article70-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article70-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "advise the Commission on any issue related to the protection of personal data in the Union, including on any proposed amendment of this Regulation;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article70,
        gdpr:article70-1,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article70(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article70 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article70-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article70-1-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "advise the Commission on the format and procedures for the exchange of information between controllers, processors and supervisory authorities for binding corporate rules;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article70,
        gdpr:article70-1,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article70(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article70 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article70-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article70-1-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "issue guidelines, recommendations, and best practices on procedures for erasing links, copies or replications of personal data from publicly available communication services as referred to in Article 17(2);"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article70,
        gdpr:article70-1,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article70(1)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article70 ;
