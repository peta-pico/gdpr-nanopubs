    GDPRtext:isPartOfChapter gdpr:chapterX .

gdpr:article92-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The delegation of power referred to in Article 12(8) and Article 43(8) shall be conferred on the Commission for an indeterminate period of time from 24 May 2016."^^xsd:string ;
