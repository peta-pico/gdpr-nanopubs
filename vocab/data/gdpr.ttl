        gdpr:article13,
        gdpr:article13-1,
        gdpr:chapterIII,
        gdpr:chapterIII-2 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article13(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article13 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article13-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-2 .

gdpr:article13-1-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "where the processing is based on point (f) of Article 6(1), the legitimate interests pursued by the controller or by a third party;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article13,
        gdpr:article13-1,
        gdpr:chapterIII,
        gdpr:chapterIII-2 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article13(1)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article13 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article13-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-2 .

gdpr:article13-1-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the recipients or categories of recipients of the personal data, if any;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article13,
        gdpr:article13-1,
        gdpr:chapterIII,
        gdpr:chapterIII-2 ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article13(1)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article13 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article13-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-2 .

gdpr:article13-1-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "where applicable, the fact that the controller intends to transfer personal data to a third country or international organisation and the existence or absence of an adequacy decision by the Commission, or in the case of transfers referred to in Article 46 or 47, or the second subparagraph of Article 49(1), reference to the appropriate or suitable safeguards and the means by which to obtain a copy of them or where they have been made available."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article13,
        gdpr:article13-1,
        gdpr:chapterIII,
        gdpr:chapterIII-2 ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article13(1)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article13 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article13-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-2 .

gdpr:article13-2-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the period for which the personal data will be stored, or if that is not possible, the criteria used to determine that period;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article13,
        gdpr:article13-2,
        gdpr:chapterIII,
        gdpr:chapterIII-2 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article13(2)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article13 ;
    GDPRtext:isPartOfChapter gdpr:chapterIII ;
    GDPRtext:isPartOfPoint gdpr:article13-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIII-2 .

gdpr:article13-2-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the existence of the right to request from the controller access to and rectification or erasure of personal data or restriction of processing concerning the data subject or to object to processing as well as the right to data portability;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article13,
        gdpr:article13-2,
        gdpr:chapterIII,
        gdpr:chapterIII-2 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article13(2)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article13 ;
