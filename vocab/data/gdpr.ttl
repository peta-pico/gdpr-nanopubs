    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article30-1-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "a description of the categories of data subjects and of the categories of personal data;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article30,
        gdpr:article30-1,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article30(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article30 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article30-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article30-1-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the categories of recipients to whom the personal data have been or will be disclosed including recipients in third countries or international organisations;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article30,
        gdpr:article30-1,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article30(1)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article30 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article30-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article30-1-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "where applicable, transfers of personal data to a third country or an international organisation, including the identification of that third country or international organisation and, in the case of transfers referred to in the second subparagraph of Article 49(1), the documentation of suitable safeguards;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article30,
        gdpr:article30-1,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article30(1)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article30 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article30-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article30-1-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "where possible, the envisaged time limits for erasure of the different categories of data;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article30,
        gdpr:article30-1,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article30(1)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article30 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article30-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article30-1-g a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "where possible, a general description of the technical and organisational security measures referred to in Article 32(1)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article30,
        gdpr:article30-1,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "g"^^xsd:string ;
    eli:title_alternative "Article30(1)(g)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article30 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article30-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article30-2-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the name and contact details of the processor or processors and of each controller on behalf of which the processor is acting, and, where applicable, of the controller's or the processor's representative, and the data protection officer;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article30,
        gdpr:article30-2,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article30(2)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article30 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article30-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article30-2-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the categories of processing carried out on behalf of each controller;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article30,
        gdpr:article30-2,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article30(2)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article30 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article30-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article30-2-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "where applicable, transfers of personal data to a third country or an international organisation, including the identification of that third country or international organisation and, in the case of transfers referred to in the second subparagraph of Article 49(1), the documentation of suitable safeguards;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article30,
        gdpr:article30-2,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article30(2)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article30 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article30-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article30-2-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "where possible, a general description of the technical and organisational security measures referred to in Article 32(1)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article30,
        gdpr:article30-2,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article30(2)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article30 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article30-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article30-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The records referred to in paragraphs 1 and 2 shall be in writing, including in electronic form."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article30,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article30(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article30 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article30-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller or the processor and, where applicable, the controller's or the processor's representative, shall make the record available to the supervisory authority on request."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article30,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article30(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article30 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article30-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The obligations referred to in paragraphs 1 and 2 shall not apply to an enterprise or an organisation employing fewer than 250 persons unless the processing it carries out is likely to result in a risk to the rights and freedoms of data subjects, the processing is not occasional, or the processing includes special categories of data as referred to in Article 9(1) or personal data relating to criminal convictions and offences referred to in Article 10."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article30,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article30(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article30 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article31-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller and the processor and, where applicable, their representatives, shall cooperate, on request, with the supervisory authority in the performance of its tasks."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article31,
        gdpr:chapterIV,
        gdpr:chapterIV-1 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article31(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article31 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-1 .

gdpr:article32-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the pseudonymisation and encryption of personal data;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article32,
        gdpr:article32-1,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article32(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article32 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article32-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article32-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the ability to ensure the ongoing confidentiality, integrity, availability and resilience of processing systems and services;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article32,
        gdpr:article32-1,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article32(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article32 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article32-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article32-1-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the ability to restore the availability and access to personal data in a timely manner in the event of a physical or technical incident;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article32,
        gdpr:article32-1,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article32(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article32 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article32-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article32-1-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "a process for regularly testing, assessing and evaluating the effectiveness of technical and organisational measures for ensuring the security of the processing."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article32,
        gdpr:article32-1,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article32(1)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article32 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article32-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article32-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "In assessing the appropriate level of security account shall be taken in particular of the risks that are presented by processing, in particular from accidental or unlawful destruction, loss, alteration, unauthorised disclosure of, or access to personal data transmitted, stored or otherwise processed."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article32,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article32(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article32 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article32-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Adherence to an approved code of conduct as referred to in Article 40 or an approved certification mechanism as referred to in Article 42 may be used as an element by which to demonstrate compliance with the requirements set out in paragraph 1 of this Article."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article32,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article32(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article32 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article32-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller and processor shall take steps to ensure that any natural person acting under the authority of the controller or the processor who has access to personal data does not process them except on instructions from the controller, unless he or she is required to do so by Union or Member State law."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article32,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article32(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article32 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article33-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "In the case of a personal data breach, the controller shall without undue delay and, where feasible, not later than 72 hours after having become aware of it, notify the personal data breach to the supervisory authority competent in accordance with Article 55, unless the personal data breach is unlikely to result in a risk to the rights and freedoms of natural persons. Where the notification to the supervisory authority is not made within 72 hours, it shall be accompanied by reasons for the delay."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article33,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article33(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article33 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article33-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The processor shall notify the controller without undue delay after becoming aware of a personal data breach."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article33,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article33(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article33 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article33-3-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "describe the nature of the personal data breach including where possible, the categories and approximate number of data subjects concerned and the categories and approximate number of personal data records concerned;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article33,
        gdpr:article33-3,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article33(3)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article33 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article33-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article33-3-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "communicate the name and contact details of the data protection officer or other contact point where more information can be obtained;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article33,
        gdpr:article33-3,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article33(3)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article33 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article33-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article33-3-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "describe the likely consequences of the personal data breach;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article33,
        gdpr:article33-3,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article33(3)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article33 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article33-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article33-3-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "describe the measures taken or proposed to be taken by the controller to address the personal data breach, including, where appropriate, measures to mitigate its possible adverse effects."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article33,
        gdpr:article33-3,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article33(3)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article33 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article33-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article33-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where, and in so far as, it is not possible to provide the information at the same time, the information may be provided in phases without undue further delay."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article33,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article33(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article33 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article33-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller shall document any personal data breaches, comprising the facts relating to the personal data breach, its effects and the remedial action taken. That documentation shall enable the supervisory authority to verify compliance with this Article."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article33,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article33(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article33 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article34-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "When the personal data breach is likely to result in a high risk to the rights and freedoms of natural persons, the controller shall communicate the personal data breach to the data subject without undue delay."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article34,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article34(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article34 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article34-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The communication to the data subject referred to in paragraph 1 of this Article shall describe in clear and plain language the nature of the personal data breach and contain at least the information and measures referred to in points (b), (c) and (d) of Article 33(3)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article34,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article34(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article34 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article34-3-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the controller has implemented appropriate technical and organisational protection measures, and those measures were applied to the personal data affected by the personal data breach, in particular those that render the personal data unintelligible to any person who is not authorised to access it, such as encryption;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article34,
        gdpr:article34-3,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article34(3)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article34 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article34-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article34-3-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the controller has taken subsequent measures which ensure that the high risk to the rights and freedoms of data subjects referred to in paragraph 1 is no longer likely to materialise;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article34,
        gdpr:article34-3,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article34(3)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article34 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article34-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article34-3-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "it would involve disproportionate effort. In such a case, there shall instead be a public communication or similar measure whereby the data subjects are informed in an equally effective manner."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article34,
        gdpr:article34-3,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article34(3)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article34 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article34-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article34-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "If the controller has not already communicated the personal data breach to the data subject, the supervisory authority, having considered the likelihood of the personal data breach resulting in a high risk, may require it to do so or may decide that any of the conditions referred to in paragraph 3 are met."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article34,
        gdpr:chapterIV,
        gdpr:chapterIV-2 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article34(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article34 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-2 .

gdpr:article35-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where a type of processing in particular using new technologies, and taking into account the nature, scope, context and purposes of the processing, is likely to result in a high risk to the rights and freedoms of natural persons, the controller shall, prior to the processing, carry out an assessment of the impact of the envisaged processing operations on the protection of personal data. A single assessment may address a set of similar processing operations that present similar high risks."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article35(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article35-10 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where processing pursuant to point (c) or (e) of Article 6(1) has a legal basis in Union law or in the law of the Member State to which the controller is subject, that law regulates the specific processing operation or set of operations in question, and a data protection impact assessment has already been carried out as part of a general impact assessment in the context of the adoption of that legal basis, paragraphs 1 to 7 shall not apply unless Member States deem it to be necessary to carry out such an assessment prior to processing activities."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "10"^^xsd:string ;
    eli:title_alternative "Article35(10)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article35-11 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where necessary, the controller shall carry out a review to assess if processing is performed in accordance with the data protection impact assessment at least when there is a change of the risk represented by processing operations."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "11"^^xsd:string ;
    eli:title_alternative "Article35(11)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article35-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller shall seek the advice of the data protection officer, where designated, when carrying out a data protection impact assessment."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article35(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article35-3-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "a systematic and extensive evaluation of personal aspects relating to natural persons which is based on automated processing, including profiling, and on which decisions are based that produce legal effects concerning the natural person or similarly significantly affect the natural person;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:article35-3,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article35(3)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article35-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article35-3-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing on a large scale of special categories of data referred to in Article 9(1), or of personal data relating to criminal convictions and offences referred to in Article 10; or"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:article35-3,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article35(3)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article35-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article35-3-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "a systematic monitoring of a publicly accessible area on a large scale."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:article35-3,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article35(3)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article35-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article35-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The supervisory authority shall establish and make public a list of the kind of processing operations which are subject to the requirement for a data protection impact assessment pursuant to paragraph 1. The supervisory authority shall communicate those lists to the Board referred to in Article 68."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article35(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article35-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The supervisory authority may also establish and make public a list of the kind of processing operations for which no data protection impact assessment is required. The supervisory authority shall communicate those lists to the Board."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article35(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article35-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Prior to the adoption of the lists referred to in paragraphs 4 and 5, the competent supervisory authority shall apply the consistency mechanism referred to in Article 63 where such lists involve processing activities which are related to the offering of goods or services to data subjects or to the monitoring of their behaviour in several Member States, or may substantially affect the free movement of personal data within the Union."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article35(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article35-7-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "a systematic description of the envisaged processing operations and the purposes of the processing, including, where applicable, the legitimate interest pursued by the controller;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:article35-7,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article35(7)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article35-7 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article35-7-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "an assessment of the necessity and proportionality of the processing operations in relation to the purposes;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:article35-7,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article35(7)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article35-7 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article35-7-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "an assessment of the risks to the rights and freedoms of data subjects referred to in paragraph 1; and"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:article35-7,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article35(7)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article35-7 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article35-7-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the measures envisaged to address the risks, including safeguards, security measures and mechanisms to ensure the protection of personal data and to demonstrate compliance with this Regulation taking into account the rights and legitimate interests of data subjects and other persons concerned."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:article35-7,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article35(7)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article35-7 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article35-8 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Compliance with approved codes of conduct referred to in Article 40 by the relevant controllers or processors shall be taken into due account in assessing the impact of the processing operations performed by such controllers or processors, in particular for the purposes of a data protection impact assessment."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "8"^^xsd:string ;
    eli:title_alternative "Article35(8)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article35-9 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where appropriate, the controller shall seek the views of data subjects or their representatives on the intended processing, without prejudice to the protection of commercial or public interests or the security of processing operations."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article35,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "9"^^xsd:string ;
    eli:title_alternative "Article35(9)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article35 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article36-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller shall consult the supervisory authority prior to processing where a data protection impact assessment under Article 35 indicates that the processing would result in a high risk in the absence of measures taken by the controller to mitigate the risk."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article36,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article36(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article36 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article36-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where the supervisory authority is of the opinion that the intended processing referred to in paragraph 1 would infringe this Regulation, in particular where the controller has insufficiently identified or mitigated the risk, the supervisory authority shall, within period of up to eight weeks of receipt of the request for consultation, provide written advice to the controller and, where applicable to the processor, and may use any of its powers referred to in Article 58. That period may be extended by six weeks, taking into account the complexity of the intended processing. The supervisory authority shall inform the controller and, where applicable, the processor, of any such extension within one month of receipt of the request for consultation together with the reasons for the delay. Those periods may be suspended until the supervisory authority has obtained information it has requested for the purposes of the consultation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article36,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article36(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article36 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article36-3-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "where applicable, the respective responsibilities of the controller, joint controllers and processors involved in the processing, in particular for processing within a group of undertakings;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article36,
        gdpr:article36-3,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article36(3)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article36 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article36-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article36-3-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the purposes and means of the intended processing;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article36,
        gdpr:article36-3,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article36(3)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article36 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article36-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article36-3-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the measures and safeguards provided to protect the rights and freedoms of data subjects pursuant to this Regulation;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article36,
        gdpr:article36-3,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article36(3)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article36 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article36-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article36-3-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "where applicable, the contact details of the data protection officer;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article36,
        gdpr:article36-3,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article36(3)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article36 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article36-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article36-3-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the data protection impact assessment provided for in Article 35; and"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article36,
        gdpr:article36-3,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article36(3)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article36 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article36-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article36-3-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "any other information requested by the supervisory authority."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article36,
        gdpr:article36-3,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article36(3)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article36 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article36-3 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article36-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Member States shall consult the supervisory authority during the preparation of a proposal for a legislative measure to be adopted by a national parliament, or of a regulatory measure based on such a legislative measure, which relates to processing."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article36,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article36(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article36 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article36-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Notwithstanding paragraph 1, Member State law may require controllers to consult with, and obtain prior authorisation from, the supervisory authority in relation to processing by a controller for the performance of a task carried out by the controller in the public interest, including processing in relation to social protection and public health."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article36,
        gdpr:chapterIV,
        gdpr:chapterIV-3 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article36(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article36 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-3 .

gdpr:article37-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the processing is carried out by a public authority or body, except for courts acting in their judicial capacity;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article37,
        gdpr:article37-1,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article37(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article37 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article37-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article37-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the core activities of the controller or the processor consist of processing operations which, by virtue of their nature, their scope and/or their purposes, require regular and systematic monitoring of data subjects on a large scale; or"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article37,
        gdpr:article37-1,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article37(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article37 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article37-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article37-1-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the core activities of the controller or the processor consist of processing on a large scale of special categories of data pursuant to Article 9 and personal data relating to criminal convictions and offences referred to in Article 10."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article37,
        gdpr:article37-1,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article37(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article37 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article37-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article37-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "A group of undertakings may appoint a single data protection officer provided that a data protection officer is easily accessible from each establishment."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article37,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article37(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article37 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article37-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where the controller or the processor is a public authority or body, a single data protection officer may be designated for several such authorities or bodies, taking account of their organisational structure and size."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article37,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article37(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article37 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article37-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "In cases other than those referred to in paragraph 1, the controller or processor or associations and other bodies representing categories of controllers or processors may or, where required by Union or Member State law shall, designate a data protection officer. The data protection officer may act for such associations and other bodies representing controllers or processors."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article37,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article37(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article37 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article37-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The data protection officer shall be designated on the basis of professional qualities and, in particular, expert knowledge of data protection law and practices and the ability to fulfil the tasks referred to in Article 39."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article37,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article37(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article37 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article37-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The data protection officer may be a staff member of the controller or processor, or fulfil the tasks on the basis of a service contract."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article37,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article37(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article37 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article37-7 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller or the processor shall publish the contact details of the data protection officer and communicate them to the supervisory authority."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article37,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "7"^^xsd:string ;
    eli:title_alternative "Article37(7)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article37 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article38-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller and the processor shall ensure that the data protection officer is involved, properly and in a timely manner, in all issues which relate to the protection of personal data."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article38,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article38(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article38 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article38-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller and processor shall support the data protection officer in performing the tasks referred to in Article 39 by providing resources necessary to carry out those tasks and access to personal data and processing operations, and to maintain his or her expert knowledge."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article38,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article38(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article38 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article38-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller and processor shall ensure that the data protection officer does not receive any instructions regarding the exercise of those tasks. He or she shall not be dismissed or penalised by the controller or the processor for performing his tasks. The data protection officer shall directly report to the highest management level of the controller or the processor."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article38,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article38(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article38 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article38-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Data subjects may contact the data protection officer with regard to all issues related to processing of their personal data and to the exercise of their rights under this Regulation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article38,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article38(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article38 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article38-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The data protection officer shall be bound by secrecy or confidentiality concerning the performance of his or her tasks, in accordance with Union or Member State law."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article38,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article38(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article38 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article38-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The data protection officer may fulfil other tasks and duties. The controller or processor shall ensure that any such tasks and duties do not result in a conflict of interests."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article38,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article38(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article38 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article39-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "to inform and advise the controller or the processor and the employees who carry out processing of their obligations pursuant to this Regulation and to other Union or Member State data protection provisions;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article39,
        gdpr:article39-1,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article39(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article39 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article39-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article39-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "to monitor compliance with this Regulation, with other Union or Member State data protection provisions and with the policies of the controller or processor in relation to the protection of personal data, including the assignment of responsibilities, awareness-raising and training of staff involved in processing operations, and the related audits;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article39,
        gdpr:article39-1,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article39(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article39 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article39-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article39-1-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "to provide advice where requested as regards the data protection impact assessment and monitor its performance pursuant to Article 35;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article39,
        gdpr:article39-1,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article39(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article39 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article39-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article39-1-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "to cooperate with the supervisory authority;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article39,
        gdpr:article39-1,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article39(1)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article39 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article39-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article39-1-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "to act as the contact point for the supervisory authority on issues relating to processing, including the prior consultation referred to in Article 36, and to consult, where appropriate, with regard to any other matter."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article39,
        gdpr:article39-1,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article39(1)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article39 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article39-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article39-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The data protection officer shall in the performance of his or her tasks have due regard to the risk associated with processing operations, taking into account the nature, scope, context and purposes of processing."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article39,
        gdpr:chapterIV,
        gdpr:chapterIV-4 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article39(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article39 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-4 .

gdpr:article4-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "For the purposes of this Regulation:"^^xsd:string,
        "‘personal data’ means any information relating to an identified or identifiable natural person (‘data subject’); an identifiable natural person is one who can be identified, directly or indirectly, in particular by reference to an identifier such as a name, an identification number, location data, an online identifier or to one or more factors specific to the physical, physiological, genetic, mental, economic, cultural or social identity of that natural person;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article4(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-10 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘third party’ means a natural or legal person, public authority, agency or body other than the data subject, controller, processor and persons who, under the direct authority of the controller or processor, are authorised to process personal data;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "10"^^xsd:string ;
    eli:title_alternative "Article4(10)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-11 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘consent’ of the data subject means any freely given, specific, informed and unambiguous indication of the data subject's wishes by which he or she, by a statement or by a clear affirmative action, signifies agreement to the processing of personal data relating to him or her;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "11"^^xsd:string ;
    eli:title_alternative "Article4(11)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-12 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘personal data breach’ means a breach of security leading to the accidental or unlawful destruction, loss, alteration, unauthorised disclosure of, or access to, personal data transmitted, stored or otherwise processed;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "12"^^xsd:string ;
    eli:title_alternative "Article4(12)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-13 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘genetic data’ means personal data relating to the inherited or acquired genetic characteristics of a natural person which give unique information about the physiology or the health of that natural person and which result, in particular, from an analysis of a biological sample from the natural person in question;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "13"^^xsd:string ;
    eli:title_alternative "Article4(13)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-14 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘biometric data’ means personal data resulting from specific technical processing relating to the physical, physiological or behavioural characteristics of a natural person, which allow or confirm the unique identification of that natural person, such as facial images or dactyloscopic data;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "14"^^xsd:string ;
    eli:title_alternative "Article4(14)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-15 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘data concerning health’ means personal data related to the physical or mental health of a natural person, including the provision of health care services, which reveal information about his or her health status;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "15"^^xsd:string ;
    eli:title_alternative "Article4(15)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-16-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "as regards a controller with establishments in more than one Member State, the place of its central administration in the Union, unless the decisions on the purposes and means of the processing of personal data are taken in another establishment of the controller in the Union and the latter establishment has the power to have such decisions implemented, in which case the establishment having taken such decisions is to be considered to be the main establishment;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:article4-16,
        gdpr:chapterI ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article4(16)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI ;
    GDPRtext:isPartOfPoint gdpr:article4-16 .

gdpr:article4-16-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "as regards a processor with establishments in more than one Member State, the place of its central administration in the Union, or, if the processor has no central administration in the Union, the establishment of the processor in the Union where the main processing activities in the context of the activities of an establishment of the processor take place to the extent that the processor is subject to specific obligations under this Regulation;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:article4-16,
        gdpr:chapterI ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article4(16)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI ;
    GDPRtext:isPartOfPoint gdpr:article4-16 .

gdpr:article4-17 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘representative’ means a natural or legal person established in the Union who, designated by the controller or processor in writing pursuant to Article 27, represents the controller or processor with regard to their respective obligations under this Regulation;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "17"^^xsd:string ;
    eli:title_alternative "Article4(17)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-18 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘enterprise’ means a natural or legal person engaged in an economic activity, irrespective of its legal form, including partnerships or associations regularly engaged in an economic activity;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "18"^^xsd:string ;
    eli:title_alternative "Article4(18)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-19 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘group of undertakings’ means a controlling undertaking and its controlled undertakings;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "19"^^xsd:string ;
    eli:title_alternative "Article4(19)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘processing’ means any operation or set of operations which is performed on personal data or on sets of personal data, whether or not by automated means, such as collection, recording, organisation, structuring, storage, adaptation or alteration, retrieval, consultation, use, disclosure by transmission, dissemination or otherwise making available, alignment or combination, restriction, erasure or destruction;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article4(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-20 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘binding corporate rules’ means personal data protection policies which are adhered to by a controller or processor established on the territory of a Member State for transfers or a set of transfers of personal data to a controller or processor in one or more third countries within a group of undertakings, or group of enterprises engaged in a joint economic activity;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "20"^^xsd:string ;
    eli:title_alternative "Article4(20)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-21 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘supervisory authority’ means an independent public authority which is established by a Member State pursuant to Article 51;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "21"^^xsd:string ;
    eli:title_alternative "Article4(21)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-22-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the controller or processor is established on the territory of the Member State of that supervisory authority;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:article4-22,
        gdpr:chapterI ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article4(22)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI ;
    GDPRtext:isPartOfPoint gdpr:article4-22 .

gdpr:article4-22-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "data subjects residing in the Member State of that supervisory authority are substantially affected or likely to be substantially affected by the processing; or"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:article4-22,
        gdpr:chapterI ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article4(22)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI ;
    GDPRtext:isPartOfPoint gdpr:article4-22 .

gdpr:article4-22-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "a complaint has been lodged with that supervisory authority;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:article4-22,
        gdpr:chapterI ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article4(22)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI ;
    GDPRtext:isPartOfPoint gdpr:article4-22 .

gdpr:article4-23-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing of personal data which takes place in the context of the activities of establishments in more than one Member State of a controller or processor in the Union where the controller or processor is established in more than one Member State; or"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:article4-23,
        gdpr:chapterI ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article4(23)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI ;
    GDPRtext:isPartOfPoint gdpr:article4-23 .

gdpr:article4-23-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing of personal data which takes place in the context of the activities of a single establishment of a controller or processor in the Union but which substantially affects or is likely to substantially affect data subjects in more than one Member State."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:article4-23,
        gdpr:chapterI ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article4(23)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI ;
    GDPRtext:isPartOfPoint gdpr:article4-23 .

gdpr:article4-24 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘relevant and reasoned objection’ means an objection to a draft decision as to whether there is an infringement of this Regulation, or whether envisaged action in relation to the controller or processor complies with this Regulation, which clearly demonstrates the significance of the risks posed by the draft decision as regards the fundamental rights and freedoms of data subjects and, where applicable, the free flow of personal data within the Union;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "24"^^xsd:string ;
    eli:title_alternative "Article4(24)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-25 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:cites gdpr:citation19 ;
    eli:description "‘information society service’ means a service as defined in point (b) of Article 1(1) of Directive (EU) 2015/1535 of the European Parliament and of the Council (19);"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "25"^^xsd:string ;
    eli:title_alternative "Article4(25)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-26 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘international organisation’ means an organisation and its subordinate bodies governed by public international law, or any other body which is set up by, or on the basis of, an agreement between two or more countries."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "26"^^xsd:string ;
    eli:title_alternative "Article4(26)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘restriction of processing’ means the marking of stored personal data with the aim of limiting their processing in the future;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article4(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘profiling’ means any form of automated processing of personal data consisting of the use of personal data to evaluate certain personal aspects relating to a natural person, in particular to analyse or predict aspects concerning that natural person's performance at work, economic situation, health, personal preferences, interests, reliability, behaviour, location or movements;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article4(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘pseudonymisation’ means the processing of personal data in such a manner that the personal data can no longer be attributed to a specific data subject without the use of additional information, provided that such additional information is kept separately and is subject to technical and organisational measures to ensure that the personal data are not attributed to an identified or identifiable natural person;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article4(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘filing system’ means any structured set of personal data which are accessible according to specific criteria, whether centralised, decentralised or dispersed on a functional or geographical basis;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article4(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-7 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘controller’ means the natural or legal person, public authority, agency or other body which, alone or jointly with others, determines the purposes and means of the processing of personal data; where the purposes and means of such processing are determined by Union or Member State law, the controller or the specific criteria for its nomination may be provided for by Union or Member State law;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "7"^^xsd:string ;
    eli:title_alternative "Article4(7)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-8 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘processor’ means a natural or legal person, public authority, agency or other body which processes personal data on behalf of the controller;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "8"^^xsd:string ;
    eli:title_alternative "Article4(8)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article4-9 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "‘recipient’ means a natural or legal person, public authority, agency or another body, to which the personal data are disclosed, whether a third party or not. However, public authorities which may receive personal data in the framework of a particular inquiry in accordance with Union or Member State law shall not be regarded as recipients; the processing of those data by those public authorities shall be in compliance with the applicable data protection rules according to the purposes of the processing;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article4,
        gdpr:chapterI ;
    eli:number "9"^^xsd:string ;
    eli:title_alternative "Article4(9)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article4 ;
    GDPRtext:isPartOfChapter gdpr:chapterI .

gdpr:article40-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Member States, the supervisory authorities, the Board and the Commission shall encourage the drawing up of codes of conduct intended to contribute to the proper application of this Regulation, taking account of the specific features of the various processing sectors and the specific needs of micro, small and medium-sized enterprises."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article40(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-10 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Commission shall ensure appropriate publicity for the approved codes which have been decided as having general validity in accordance with paragraph 9."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "10"^^xsd:string ;
    eli:title_alternative "Article40(10)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-11 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Board shall collate all approved codes of conduct, amendments and extensions in a register and shall make them publicly available by way of appropriate means."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "11"^^xsd:string ;
    eli:title_alternative "Article40(11)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-2-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "fair and transparent processing;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:article40-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article40(2)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article40-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-2-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the legitimate interests pursued by controllers in specific contexts;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:article40-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article40(2)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article40-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-2-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the collection of personal data;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:article40-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article40(2)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article40-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-2-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the pseudonymisation of personal data;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:article40-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article40(2)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article40-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-2-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the information provided to the public and to data subjects;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:article40-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article40(2)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article40-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-2-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the exercise of the rights of data subjects;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:article40-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article40(2)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article40-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-2-g a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the information provided to, and the protection of, children, and the manner in which the consent of the holders of parental responsibility over children is to be obtained;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:article40-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "g"^^xsd:string ;
    eli:title_alternative "Article40(2)(g)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article40-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-2-h a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the measures and procedures referred to in Articles 24 and 25 and the measures to ensure security of processing referred to in Article 32;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:article40-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "h"^^xsd:string ;
    eli:title_alternative "Article40(2)(h)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article40-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-2-i a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the notification of personal data breaches to supervisory authorities and the communication of such personal data breaches to data subjects;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:article40-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "i"^^xsd:string ;
    eli:title_alternative "Article40(2)(i)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article40-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-2-j a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the transfer of personal data to third countries or international organisations; or"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:article40-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "j"^^xsd:string ;
    eli:title_alternative "Article40(2)(j)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article40-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-2-k a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "out-of-court proceedings and other dispute resolution procedures for resolving disputes between controllers and data subjects with regard to processing, without prejudice to the rights of data subjects pursuant to Articles 77 and 79."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:article40-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "k"^^xsd:string ;
    eli:title_alternative "Article40(2)(k)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article40-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "In addition to adherence by controllers or processors subject to this Regulation, codes of conduct approved pursuant to paragraph 5 of this Article and having general validity pursuant to paragraph 9 of this Article may also be adhered to by controllers or processors that are not subject to this Regulation pursuant to Article 3 in order to provide appropriate safeguards within the framework of personal data transfers to third countries or international organisations under the terms referred to in point (e) of Article 46(2). Such controllers or processors shall make binding and enforceable commitments, via contractual or other legally binding instruments, to apply those appropriate safeguards including with regard to the rights of data subjects."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article40(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "A code of conduct referred to in paragraph 2 of this Article shall contain mechanisms which enable the body referred to in Article 41(1) to carry out the mandatory monitoring of compliance with its provisions by the controllers or processors which undertake to apply it, without prejudice to the tasks and powers of supervisory authorities competent pursuant to Article 55 or 56."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article40(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Associations and other bodies referred to in paragraph 2 of this Article which intend to prepare a code of conduct or to amend or extend an existing code shall submit the draft code, amendment or extension to the supervisory authority which is competent pursuant to Article 55. The supervisory authority shall provide an opinion on whether the draft code, amendment or extension complies with this Regulation and shall approve that draft code, amendment or extension if it finds that it provides sufficient appropriate safeguards."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article40(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where the draft code, or amendment or extension is approved in accordance with paragraph 5, and where the code of conduct concerned does not relate to processing activities in several Member States, the supervisory authority shall register and publish the code."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article40(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-7 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where a draft code of conduct relates to processing activities in several Member States, the supervisory authority which is competent pursuant to Article 55 shall, before approving the draft code, amendment or extension, submit it in the procedure referred to in Article 63 to the Board which shall provide an opinion on whether the draft code, amendment or extension complies with this Regulation or, in the situation referred to in paragraph 3 of this Article, provides appropriate safeguards."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "7"^^xsd:string ;
    eli:title_alternative "Article40(7)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-8 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where the opinion referred to in paragraph 7 confirms that the draft code, amendment or extension complies with this Regulation, or, in the situation referred to in paragraph 3, provides appropriate safeguards, the Board shall submit its opinion to the Commission."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "8"^^xsd:string ;
    eli:title_alternative "Article40(8)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article40-9 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Commission may, by way of implementing acts, decide that the approved code of conduct, amendment or extension submitted to it pursuant to paragraph 8 of this Article have general validity within the Union. Those implementing acts shall be adopted in accordance with the examination procedure set out in Article 93(2)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article40,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "9"^^xsd:string ;
    eli:title_alternative "Article40(9)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article40 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article41-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Without prejudice to the tasks and powers of the competent supervisory authority under Articles 57 and 58, the monitoring of compliance with a code of conduct pursuant to Article 40 may be carried out by a body which has an appropriate level of expertise in relation to the subject-matter of the code and is accredited for that purpose by the competent supervisory authority."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article41,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article41(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article41 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article41-2-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "demonstrated its independence and expertise in relation to the subject-matter of the code to the satisfaction of the competent supervisory authority;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article41,
        gdpr:article41-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article41(2)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article41 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article41-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article41-2-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "established procedures which allow it to assess the eligibility of controllers and processors concerned to apply the code, to monitor their compliance with its provisions and to periodically review its operation;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article41,
        gdpr:article41-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article41(2)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article41 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article41-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article41-2-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "established procedures and structures to handle complaints about infringements of the code or the manner in which the code has been, or is being, implemented by a controller or processor, and to make those procedures and structures transparent to data subjects and the public; and"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article41,
        gdpr:article41-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article41(2)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article41 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article41-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article41-2-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "demonstrated to the satisfaction of the competent supervisory authority that its tasks and duties do not result in a conflict of interests."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article41,
        gdpr:article41-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article41(2)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article41 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article41-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article41-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The competent supervisory authority shall submit the draft criteria for accreditation of a body as referred to in paragraph 1 of this Article to the Board pursuant to the consistency mechanism referred to in Article 63."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article41,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article41(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article41 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article41-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Without prejudice to the tasks and powers of the competent supervisory authority and the provisions of Chapter VIII, a body as referred to in paragraph 1 of this Article shall, subject to appropriate safeguards, take appropriate action in cases of infringement of the code by a controller or processor, including suspension or exclusion of the controller or processor concerned from the code. It shall inform the competent supervisory authority of such actions and the reasons for taking them."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article41,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article41(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article41 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article41-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The competent supervisory authority shall revoke the accreditation of a body as referred to in paragraph 1 if the conditions for accreditation are not, or are no longer, met or where actions taken by the body infringe this Regulation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article41,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article41(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article41 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article41-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "This Article shall not apply to processing carried out by public authorities and bodies."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article41,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article41(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article41 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article42-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Member States, the supervisory authorities, the Board and the Commission shall encourage, in particular at Union level, the establishment of data protection certification mechanisms and of data protection seals and marks, for the purpose of demonstrating compliance with this Regulation of processing operations by controllers and processors. The specific needs of micro, small and medium-sized enterprises shall be taken into account."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article42,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article42(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article42 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article42-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "In addition to adherence by controllers or processors subject to this Regulation, data protection certification mechanisms, seals or marks approved pursuant to paragraph 5 of this Article may be established for the purpose of demonstrating the existence of appropriate safeguards provided by controllers or processors that are not subject to this Regulation pursuant to Article 3 within the framework of personal data transfers to third countries or international organisations under the terms referred to in point (f) of Article 46(2). Such controllers or processors shall make binding and enforceable commitments, via contractual or other legally binding instruments, to apply those appropriate safeguards, including with regard to the rights of data subjects."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article42,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article42(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article42 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article42-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The certification shall be voluntary and available via a process that is transparent."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article42,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article42(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article42 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article42-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "A certification pursuant to this Article does not reduce the responsibility of the controller or the processor for compliance with this Regulation and is without prejudice to the tasks and powers of the supervisory authorities which are competent pursuant to Article 55 or 56."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article42,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article42(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article42 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article42-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "A certification pursuant to this Article shall be issued by the certification bodies referred to in Article 43 or by the competent supervisory authority, on the basis of criteria approved by that competent supervisory authority pursuant to Article 58(3) or by the Board pursuant to Article 63. Where the criteria are approved by the Board, this may result in a common certification, the European Data Protection Seal."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article42,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article42(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article42 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article42-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller or processor which submits its processing to the certification mechanism shall provide the certification body referred to in Article 43, or where applicable, the competent supervisory authority, with all information and access to its processing activities which are necessary to conduct the certification procedure."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article42,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article42(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article42 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article42-7 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Certification shall be issued to a controller or processor for a maximum period of three years and may be renewed, under the same conditions, provided that the relevant requirements continue to be met. Certification shall be withdrawn, as applicable, by the certification bodies referred to in Article 43 or by the competent supervisory authority where the requirements for the certification are not or are no longer met."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article42,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "7"^^xsd:string ;
    eli:title_alternative "Article42(7)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article42 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article42-8 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Board shall collate all certification mechanisms and data protection seals and marks in a register and shall make them publicly available by any appropriate means."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article42,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "8"^^xsd:string ;
    eli:title_alternative "Article42(8)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article42 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article43-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the supervisory authority which is competent pursuant to Article 55 or 56;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article43,
        gdpr:article43-1,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article43(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article43 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article43-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article43-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the national accreditation body named in accordance with Regulation (EC) No 765/2008 of the European Parliament and of the Council (20) in accordance with EN-ISO/IEC 17065/2012 and with the additional requirements established by the supervisory authority which is competent pursuant to Article 55 or 56."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article43,
        gdpr:article43-1,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article43(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article43 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article43-1 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article43-2-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "demonstrated their independence and expertise in relation to the subject-matter of the certification to the satisfaction of the competent supervisory authority;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article43,
        gdpr:article43-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article43(2)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article43 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article43-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article43-2-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "undertaken to respect the criteria referred to in Article 42(5) and approved by the supervisory authority which is competent pursuant to Article 55 or 56 or by the Board pursuant to Article 63;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article43,
        gdpr:article43-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article43(2)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article43 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article43-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article43-2-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "established procedures for the issuing, periodic review and withdrawal of data protection certification, seals and marks;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article43,
        gdpr:article43-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article43(2)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article43 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article43-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article43-2-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "established procedures and structures to handle complaints about infringements of the certification or the manner in which the certification has been, or is being, implemented by the controller or processor, and to make those procedures and structures transparent to data subjects and the public; and"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article43,
        gdpr:article43-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article43(2)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article43 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article43-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article43-2-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "demonstrated, to the satisfaction of the competent supervisory authority, that their tasks and duties do not result in a conflict of interests."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article43,
        gdpr:article43-2,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article43(2)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article43 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfPoint gdpr:article43-2 ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article43-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The accreditation of certification bodies as referred to in paragraphs 1 and 2 of this Article shall take place on the basis of criteria approved by the supervisory authority which is competent pursuant to Article 55 or 56 or by the Board pursuant to Article 63. In the case of accreditation pursuant to point (b) of paragraph 1 of this Article, those requirements shall complement those envisaged in Regulation (EC) No 765/2008 and the technical rules that describe the methods and procedures of the certification bodies."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article43,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article43(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article43 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article43-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The certification bodies referred to in paragraph 1 shall be responsible for the proper assessment leading to the certification or the withdrawal of such certification without prejudice to the responsibility of the controller or processor for compliance with this Regulation. The accreditation shall be issued for a maximum period of five years and may be renewed on the same conditions provided that the certification body meets the requirements set out in this Article."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article43,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article43(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article43 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article43-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The certification bodies referred to in paragraph 1 shall provide the competent supervisory authorities with the reasons for granting or withdrawing the requested certification."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article43,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article43(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article43 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article43-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The requirements referred to in paragraph 3 of this Article and the criteria referred to in Article 42(5) shall be made public by the supervisory authority in an easily accessible form. The supervisory authorities shall also transmit those requirements and criteria to the Board. The Board shall collate all certification mechanisms and data protection seals in a register and shall make them publicly available by any appropriate means."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article43,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article43(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article43 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article43-7 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Without prejudice to Chapter VIII, the competent supervisory authority or the national accreditation body shall revoke an accreditation of a certification body pursuant to paragraph 1 of this Article where the conditions for the accreditation are not, or are no longer, met or where actions taken by a certification body infringe this Regulation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article43,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "7"^^xsd:string ;
    eli:title_alternative "Article43(7)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article43 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article43-8 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Commission shall be empowered to adopt delegated acts in accordance with Article 92 for the purpose of specifying the requirements to be taken into account for the data protection certification mechanisms referred to in Article 42(1)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article43,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "8"^^xsd:string ;
    eli:title_alternative "Article43(8)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article43 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article43-9 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Commission may adopt implementing acts laying down technical standards for certification mechanisms and data protection seals and marks, and mechanisms to promote and recognise those certification mechanisms, seals and marks. Those implementing acts shall be adopted in accordance with the examination procedure referred to in Article 93(2)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article43,
        gdpr:chapterIV,
        gdpr:chapterIV-5 ;
    eli:number "9"^^xsd:string ;
    eli:title_alternative "Article43(9)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article43 ;
    GDPRtext:isPartOfChapter gdpr:chapterIV ;
    GDPRtext:isPartOfSection gdpr:chapterIV-5 .

gdpr:article44-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Any transfer of personal data which are undergoing processing or are intended for processing after transfer to a third country or to an international organisation shall take place only if, subject to the other provisions of this Regulation, the conditions laid down in this Chapter are complied with by the controller and processor, including for onward transfers of personal data from the third country or an international organisation to another third country or to another international organisation. All provisions in this Chapter shall be applied in order to ensure that the level of protection of natural persons guaranteed by this Regulation is not undermined."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article44,
        gdpr:chapterV ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article44(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article44 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article45-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "A transfer of personal data to a third country or an international organisation may take place where the Commission has decided that the third country, a territory or one or more specified sectors within that third country, or the international organisation in question ensures an adequate level of protection. Such a transfer shall not require any specific authorisation."^^xsd:string,
        "On duly justified imperative grounds of urgency, the Commission shall adopt immediately applicable implementing acts in accordance with the procedure referred to in Article 93(3)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article45,
        gdpr:chapterV ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article45(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article45 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article45-2-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the rule of law, respect for human rights and fundamental freedoms, relevant legislation, both general and sectoral, including concerning public security, defence, national security and criminal law and the access of public authorities to personal data, as well as the implementation of such legislation, data protection rules, professional rules and security measures, including rules for the onward transfer of personal data to another third country or international organisation which are complied with in that country or international organisation, case-law, as well as effective and enforceable data subject rights and effective administrative and judicial redress for the data subjects whose personal data are being transferred;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article45,
        gdpr:article45-2,
        gdpr:chapterV ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article45(2)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article45 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article45-2 .

gdpr:article45-2-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the existence and effective functioning of one or more independent supervisory authorities in the third country or to which an international organisation is subject, with responsibility for ensuring and enforcing compliance with the data protection rules, including adequate enforcement powers, for assisting and advising the data subjects in exercising their rights and for cooperation with the supervisory authorities of the Member States; and"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article45,
        gdpr:article45-2,
        gdpr:chapterV ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article45(2)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article45 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article45-2 .

gdpr:article45-2-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the international commitments the third country or international organisation concerned has entered into, or other obligations arising from legally binding conventions or instruments as well as from its participation in multilateral or regional systems, in particular in relation to the protection of personal data."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article45,
        gdpr:article45-2,
        gdpr:chapterV ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article45(2)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article45 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article45-2 .

gdpr:article45-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Commission, after assessing the adequacy of the level of protection, may decide, by means of implementing act, that a third country, a territory or one or more specified sectors within a third country, or an international organisation ensures an adequate level of protection within the meaning of paragraph 2 of this Article. The implementing act shall provide for a mechanism for a periodic review, at least every four years, which shall take into account all relevant developments in the third country or international organisation. The implementing act shall specify its territorial and sectoral application and, where applicable, identify the supervisory authority or authorities referred to in point (b) of paragraph 2 of this Article. The implementing act shall be adopted in accordance with the examination procedure referred to in Article 93(2)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article45,
        gdpr:chapterV ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article45(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article45 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article45-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Commission shall, on an ongoing basis, monitor developments in third countries and international organisations that could affect the functioning of decisions adopted pursuant to paragraph 3 of this Article and decisions adopted on the basis of Article 25(6) of Directive 95/46/EC."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article45,
        gdpr:chapterV ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article45(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article45 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article45-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Commission shall, where available information reveals, in particular following the review referred to in paragraph 3 of this Article, that a third country, a territory or one or more specified sectors within a third country, or an international organisation no longer ensures an adequate level of protection within the meaning of paragraph 2 of this Article, to the extent necessary, repeal, amend or suspend the decision referred to in paragraph 3 of this Article by means of implementing acts without retro-active effect. Those implementing acts shall be adopted in accordance with the examination procedure referred to in Article 93(2)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article45,
        gdpr:chapterV ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article45(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article45 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article45-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Commission shall enter into consultations with the third country or international organisation with a view to remedying the situation giving rise to the decision made pursuant to paragraph 5."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article45,
        gdpr:chapterV ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article45(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article45 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article45-7 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "A decision pursuant to paragraph 5 of this Article is without prejudice to transfers of personal data to the third country, a territory or one or more specified sectors within that third country, or the international organisation in question pursuant to Articles 46 to 49."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article45,
        gdpr:chapterV ;
    eli:number "7"^^xsd:string ;
    eli:title_alternative "Article45(7)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article45 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article45-8 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Commission shall publish in the Official Journal of the European Union and on its website a list of the third countries, territories and specified sectors within a third country and international organisations for which it has decided that an adequate level of protection is or is no longer ensured."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article45,
        gdpr:chapterV ;
    eli:number "8"^^xsd:string ;
    eli:title_alternative "Article45(8)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article45 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article45-9 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Decisions adopted by the Commission on the basis of Article 25(6) of Directive 95/46/EC shall remain in force until amended, replaced or repealed by a Commission Decision adopted in accordance with paragraph 3 or 5 of this Article."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article45,
        gdpr:chapterV ;
    eli:number "9"^^xsd:string ;
    eli:title_alternative "Article45(9)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article45 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article46-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "In the absence of a decision pursuant to Article 45(3), a controller or processor may transfer personal data to a third country or an international organisation only if the controller or processor has provided appropriate safeguards, and on condition that enforceable data subject rights and effective legal remedies for data subjects are available."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article46,
        gdpr:chapterV ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article46(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article46 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article46-2-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "a legally binding and enforceable instrument between public authorities or bodies;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article46,
        gdpr:article46-2,
        gdpr:chapterV ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article46(2)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article46 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article46-2 .

gdpr:article46-2-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "binding corporate rules in accordance with Article 47;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article46,
        gdpr:article46-2,
        gdpr:chapterV ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article46(2)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article46 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article46-2 .

gdpr:article46-2-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "standard data protection clauses adopted by the Commission in accordance with the examination procedure referred to in Article 93(2);"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article46,
        gdpr:article46-2,
        gdpr:chapterV ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article46(2)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article46 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article46-2 .

gdpr:article46-2-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "standard data protection clauses adopted by a supervisory authority and approved by the Commission pursuant to the examination procedure referred to in Article 93(2);"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article46,
        gdpr:article46-2,
        gdpr:chapterV ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article46(2)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article46 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article46-2 .

gdpr:article46-2-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "an approved code of conduct pursuant to Article 40 together with binding and enforceable commitments of the controller or processor in the third country to apply the appropriate safeguards, including as regards data subjects' rights; or"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article46,
        gdpr:article46-2,
        gdpr:chapterV ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article46(2)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article46 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article46-2 .

gdpr:article46-2-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "an approved certification mechanism pursuant to Article 42 together with binding and enforceable commitments of the controller or processor in the third country to apply the appropriate safeguards, including as regards data subjects' rights."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article46,
        gdpr:article46-2,
        gdpr:chapterV ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article46(2)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article46 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article46-2 .

gdpr:article46-3-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "contractual clauses between the controller or processor and the controller, processor or the recipient of the personal data in the third country or international organisation; or"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article46,
        gdpr:article46-3,
        gdpr:chapterV ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article46(3)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article46 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article46-3 .

gdpr:article46-3-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "provisions to be inserted into administrative arrangements between public authorities or bodies which include enforceable and effective data subject rights."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article46,
        gdpr:article46-3,
        gdpr:chapterV ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article46(3)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article46 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article46-3 .

gdpr:article46-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The supervisory authority shall apply the consistency mechanism referred to in Article 63 in the cases referred to in paragraph 3 of this Article."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article46,
        gdpr:chapterV ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article46(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article46 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article46-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Authorisations by a Member State or supervisory authority on the basis of Article 26(2) of Directive 95/46/EC shall remain valid until amended, replaced or repealed, if necessary, by that supervisory authority. Decisions adopted by the Commission on the basis of Article 26(4) of Directive 95/46/EC shall remain in force until amended, replaced or repealed, if necessary, by a Commission Decision adopted in accordance with paragraph 2 of this Article."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article46,
        gdpr:chapterV ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article46(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article46 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article47-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "are legally binding and apply to and are enforced by every member concerned of the group of undertakings, or group of enterprises engaged in a joint economic activity, including their employees;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-1,
        gdpr:chapterV ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article47(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-1 .

gdpr:article47-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "expressly confer enforceable rights on data subjects with regard to the processing of their personal data; and"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-1,
        gdpr:chapterV ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article47(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-1 .

gdpr:article47-1-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "fulfil the requirements laid down in paragraph 2."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-1,
        gdpr:chapterV ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article47(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-1 .

gdpr:article47-2-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the structure and contact details of the group of undertakings, or group of enterprises engaged in a joint economic activity and of each of its members;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-2,
        gdpr:chapterV ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article47(2)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-2 .

gdpr:article47-2-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the data transfers or set of transfers, including the categories of personal data, the type of processing and its purposes, the type of data subjects affected and the identification of the third country or countries in question;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-2,
        gdpr:chapterV ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article47(2)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-2 .

gdpr:article47-2-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "their legally binding nature, both internally and externally;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-2,
        gdpr:chapterV ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article47(2)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-2 .

gdpr:article47-2-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the application of the general data protection principles, in particular purpose limitation, data minimisation, limited storage periods, data quality, data protection by design and by default, legal basis for processing, processing of special categories of personal data, measures to ensure data security, and the requirements in respect of onward transfers to bodies not bound by the binding corporate rules;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-2,
        gdpr:chapterV ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article47(2)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-2 .

gdpr:article47-2-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the rights of data subjects in regard to processing and the means to exercise those rights, including the right not to be subject to decisions based solely on automated processing, including profiling in accordance with Article 22, the right to lodge a complaint with the competent supervisory authority and before the competent courts of the Member States in accordance with Article 79, and to obtain redress and, where appropriate, compensation for a breach of the binding corporate rules;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-2,
        gdpr:chapterV ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article47(2)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-2 .

gdpr:article47-2-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the acceptance by the controller or processor established on the territory of a Member State of liability for any breaches of the binding corporate rules by any member concerned not established in the Union; the controller or the processor shall be exempt from that liability, in whole or in part, only if it proves that that member is not responsible for the event giving rise to the damage;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-2,
        gdpr:chapterV ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article47(2)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-2 .

gdpr:article47-2-g a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "how the information on the binding corporate rules, in particular on the provisions referred to in points (d), (e) and (f) of this paragraph is provided to the data subjects in addition to Articles 13 and 14;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-2,
        gdpr:chapterV ;
    eli:number "g"^^xsd:string ;
    eli:title_alternative "Article47(2)(g)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-2 .

gdpr:article47-2-h a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the tasks of any data protection officer designated in accordance with Article 37 or any other person or entity in charge of the monitoring compliance with the binding corporate rules within the group of undertakings, or group of enterprises engaged in a joint economic activity, as well as monitoring training and complaint-handling;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-2,
        gdpr:chapterV ;
    eli:number "h"^^xsd:string ;
    eli:title_alternative "Article47(2)(h)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-2 .

gdpr:article47-2-i a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the complaint procedures;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-2,
        gdpr:chapterV ;
    eli:number "i"^^xsd:string ;
    eli:title_alternative "Article47(2)(i)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-2 .

gdpr:article47-2-j a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the mechanisms within the group of undertakings, or group of enterprises engaged in a joint economic activity for ensuring the verification of compliance with the binding corporate rules. Such mechanisms shall include data protection audits and methods for ensuring corrective actions to protect the rights of the data subject. Results of such verification should be communicated to the person or entity referred to in point (h) and to the board of the controlling undertaking of a group of undertakings, or of the group of enterprises engaged in a joint economic activity, and should be available upon request to the competent supervisory authority;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-2,
        gdpr:chapterV ;
    eli:number "j"^^xsd:string ;
    eli:title_alternative "Article47(2)(j)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-2 .

gdpr:article47-2-k a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the mechanisms for reporting and recording changes to the rules and reporting those changes to the supervisory authority;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-2,
        gdpr:chapterV ;
    eli:number "k"^^xsd:string ;
    eli:title_alternative "Article47(2)(k)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-2 .

gdpr:article47-2-l a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the cooperation mechanism with the supervisory authority to ensure compliance by any member of the group of undertakings, or group of enterprises engaged in a joint economic activity, in particular by making available to the supervisory authority the results of verifications of the measures referred to in point (j);"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-2,
        gdpr:chapterV ;
    eli:number "l"^^xsd:string ;
    eli:title_alternative "Article47(2)(l)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-2 .

gdpr:article47-2-m a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the mechanisms for reporting to the competent supervisory authority any legal requirements to which a member of the group of undertakings, or group of enterprises engaged in a joint economic activity is subject in a third country which are likely to have a substantial adverse effect on the guarantees provided by the binding corporate rules; and"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-2,
        gdpr:chapterV ;
    eli:number "m"^^xsd:string ;
    eli:title_alternative "Article47(2)(m)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-2 .

gdpr:article47-2-n a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the appropriate data protection training to personnel having permanent or regular access to personal data."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:article47-2,
        gdpr:chapterV ;
    eli:number "n"^^xsd:string ;
    eli:title_alternative "Article47(2)(n)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article47-2 .

gdpr:article47-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The Commission may specify the format and procedures for the exchange of information between controllers, processors and supervisory authorities for binding corporate rules within the meaning of this Article. Those implementing acts shall be adopted in accordance with the examination procedure set out in Article 93(2)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article47,
        gdpr:chapterV ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article47(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article47 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article48-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Any judgment of a court or tribunal and any decision of an administrative authority of a third country requiring a controller or processor to transfer or disclose personal data may only be recognised or enforceable in any manner if based on an international agreement, such as a mutual legal assistance treaty, in force between the requesting third country and the Union or a Member State, without prejudice to other grounds for transfer pursuant to this Chapter."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article48,
        gdpr:chapterV ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article48(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article48 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article49-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the data subject has explicitly consented to the proposed transfer, after having been informed of the possible risks of such transfers for the data subject due to the absence of an adequacy decision and appropriate safeguards;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article49,
        gdpr:article49-1,
        gdpr:chapterV ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article49(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article49 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article49-1 .

gdpr:article49-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the transfer is necessary for the performance of a contract between the data subject and the controller or the implementation of pre-contractual measures taken at the data subject's request;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article49,
        gdpr:article49-1,
        gdpr:chapterV ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article49(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article49 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article49-1 .

gdpr:article49-1-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the transfer is necessary for the conclusion or performance of a contract concluded in the interest of the data subject between the controller and another natural or legal person;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article49,
        gdpr:article49-1,
        gdpr:chapterV ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article49(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article49 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article49-1 .

gdpr:article49-1-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the transfer is necessary for important reasons of public interest;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article49,
        gdpr:article49-1,
        gdpr:chapterV ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article49(1)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article49 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article49-1 .

gdpr:article49-1-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the transfer is necessary for the establishment, exercise or defence of legal claims;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article49,
        gdpr:article49-1,
        gdpr:chapterV ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article49(1)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article49 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article49-1 .

gdpr:article49-1-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the transfer is necessary in order to protect the vital interests of the data subject or of other persons, where the data subject is physically or legally incapable of giving consent;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article49,
        gdpr:article49-1,
        gdpr:chapterV ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article49(1)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article49 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article49-1 .

gdpr:article49-1-g a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the transfer is made from a register which according to Union or Member State law is intended to provide information to the public and which is open to consultation either by the public in general or by any person who can demonstrate a legitimate interest, but only to the extent that the conditions laid down by Union or Member State law for consultation are fulfilled in the particular case."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article49,
        gdpr:article49-1,
        gdpr:chapterV ;
    eli:number "g"^^xsd:string ;
    eli:title_alternative "Article49(1)(g)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article49 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article49-1 .

gdpr:article49-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "A transfer pursuant to point (g) of the first subparagraph of paragraph 1 shall not involve the entirety of the personal data or entire categories of the personal data contained in the register. Where the register is intended for consultation by persons having a legitimate interest, the transfer shall be made only at the request of those persons or if they are to be the recipients."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article49,
        gdpr:chapterV ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article49(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article49 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article49-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Points (a), (b) and (c) of the first subparagraph of paragraph 1 and the second subparagraph thereof shall not apply to activities carried out by public authorities in the exercise of their public powers."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article49,
        gdpr:chapterV ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article49(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article49 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article49-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The public interest referred to in point (d) of the first subparagraph of paragraph 1 shall be recognised in Union law or in the law of the Member State to which the controller is subject."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article49,
        gdpr:chapterV ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article49(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article49 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article49-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "In the absence of an adequacy decision, Union or Member State law may, for important reasons of public interest, expressly set limits to the transfer of specific categories of personal data to a third country or an international organisation. Member States shall notify such provisions to the Commission."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article49,
        gdpr:chapterV ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article49(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article49 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article49-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller or processor shall document the assessment as well as the suitable safeguards referred to in the second subparagraph of paragraph 1 of this Article in the records referred to in Article 30."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article49,
        gdpr:chapterV ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article49(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article49 ;
    GDPRtext:isPartOfChapter gdpr:chapterV .

gdpr:article5-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processed lawfully, fairly and in a transparent manner in relation to the data subject (‘lawfulness, fairness and transparency’);"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article5,
        gdpr:article5-1,
        gdpr:chapterII ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article5(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article5 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article5-1 .

gdpr:article5-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "collected for specified, explicit and legitimate purposes and not further processed in a manner that is incompatible with those purposes; further processing for archiving purposes in the public interest, scientific or historical research purposes or statistical purposes shall, in accordance with Article 89(1), not be considered to be incompatible with the initial purposes (‘purpose limitation’);"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article5,
        gdpr:article5-1,
        gdpr:chapterII ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article5(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article5 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article5-1 .

gdpr:article5-1-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "adequate, relevant and limited to what is necessary in relation to the purposes for which they are processed (‘data minimisation’);"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article5,
        gdpr:article5-1,
        gdpr:chapterII ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article5(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article5 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article5-1 .

gdpr:article5-1-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "accurate and, where necessary, kept up to date; every reasonable step must be taken to ensure that personal data that are inaccurate, having regard to the purposes for which they are processed, are erased or rectified without delay (‘accuracy’);"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article5,
        gdpr:article5-1,
        gdpr:chapterII ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article5(1)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article5 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article5-1 .

gdpr:article5-1-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "kept in a form which permits identification of data subjects for no longer than is necessary for the purposes for which the personal data are processed; personal data may be stored for longer periods insofar as the personal data will be processed solely for archiving purposes in the public interest, scientific or historical research purposes or statistical purposes in accordance with Article 89(1) subject to implementation of the appropriate technical and organisational measures required by this Regulation in order to safeguard the rights and freedoms of the data subject (‘storage limitation’);"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article5,
        gdpr:article5-1,
        gdpr:chapterII ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article5(1)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article5 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article5-1 .

gdpr:article5-1-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processed in a manner that ensures appropriate security of the personal data, including protection against unauthorised or unlawful processing and against accidental loss, destruction or damage, using appropriate technical or organisational measures (‘integrity and confidentiality’)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article5,
        gdpr:article5-1,
        gdpr:chapterII ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article5(1)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article5 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article5-1 .

gdpr:article5-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller shall be responsible for, and be able to demonstrate compliance with, paragraph 1 (‘accountability’)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article5,
        gdpr:chapterII ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article5(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article5 ;
    GDPRtext:isPartOfChapter gdpr:chapterII .

gdpr:article50-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "develop international cooperation mechanisms to facilitate the effective enforcement of legislation for the protection of personal data;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article50,
        gdpr:article50-1,
        gdpr:chapterV ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article50(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article50 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article50-1 .

gdpr:article50-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "provide international mutual assistance in the enforcement of legislation for the protection of personal data, including through notification, complaint referral, investigative assistance and information exchange, subject to appropriate safeguards for the protection of personal data and other fundamental rights and freedoms;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article50,
        gdpr:article50-1,
        gdpr:chapterV ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article50(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article50 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article50-1 .

gdpr:article50-1-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "engage relevant stakeholders in discussion and activities aimed at furthering international cooperation in the enforcement of legislation for the protection of personal data;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article50,
        gdpr:article50-1,
        gdpr:chapterV ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article50(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article50 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article50-1 .

gdpr:article50-1-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "promote the exchange and documentation of personal data protection legislation and practice, including on jurisdictional conflicts with third countries."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article50,
        gdpr:article50-1,
        gdpr:chapterV ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article50(1)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article50 ;
    GDPRtext:isPartOfChapter gdpr:chapterV ;
    GDPRtext:isPartOfPoint gdpr:article50-1 .

gdpr:article51-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each Member State shall provide for one or more independent public authorities to be responsible for monitoring the application of this Regulation, in order to protect the fundamental rights and freedoms of natural persons in relation to processing and to facilitate the free flow of personal data within the Union (‘supervisory authority’)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article51,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article51(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article51 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article51-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each supervisory authority shall contribute to the consistent application of this Regulation throughout the Union. For that purpose, the supervisory authorities shall cooperate with each other and the Commission in accordance with Chapter VII."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article51,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article51(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article51 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article51-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where more than one supervisory authority is established in a Member State, that Member State shall designate the supervisory authority which is to represent those authorities in the Board and shall set out the mechanism to ensure compliance by the other authorities with the rules relating to the consistency mechanism referred to in Article 63."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article51,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article51(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article51 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article51-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each Member State shall notify to the Commission the provisions of its law which it adopts pursuant to this Chapter, by 25 May 2018 and, without delay, any subsequent amendment affecting them."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article51,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article51(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article51 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article52-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each supervisory authority shall act with complete independence in performing its tasks and exercising its powers in accordance with this Regulation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article52,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article52(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article52 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article52-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The member or members of each supervisory authority shall, in the performance of their tasks and exercise of their powers in accordance with this Regulation, remain free from external influence, whether direct or indirect, and shall neither seek nor take instructions from anybody."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article52,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article52(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article52 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article52-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Member or members of each supervisory authority shall refrain from any action incompatible with their duties and shall not, during their term of office, engage in any incompatible occupation, whether gainful or not."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article52,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article52(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article52 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article52-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each Member State shall ensure that each supervisory authority is provided with the human, technical and financial resources, premises and infrastructure necessary for the effective performance of its tasks and exercise of its powers, including those to be carried out in the context of mutual assistance, cooperation and participation in the Board."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article52,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article52(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article52 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article52-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each Member State shall ensure that each supervisory authority chooses and has its own staff which shall be subject to the exclusive direction of the member or members of the supervisory authority concerned."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article52,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article52(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article52 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article52-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each Member State shall ensure that each supervisory authority is subject to financial control which does not affect its independence and that it has separate, public annual budgets, which may be part of the overall state or national budget."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article52,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article52(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article52 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article53-1-1 a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "their parliament;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article53,
        gdpr:article53-1,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article53(1)(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article53 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfPoint gdpr:article53-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article53-1-2 a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "their government;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article53,
        gdpr:article53-1,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article53(1)(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article53 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfPoint gdpr:article53-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article53-1-3 a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "their head of State; or"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article53,
        gdpr:article53-1,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article53(1)(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article53 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfPoint gdpr:article53-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article53-1-4 a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "an independent body entrusted with the appointment under Member State law."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article53,
        gdpr:article53-1,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article53(1)(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article53 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfPoint gdpr:article53-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article53-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each member shall have the qualifications, experience and skills, in particular in the area of the protection of personal data, required to perform its duties and exercise its powers."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article53,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article53(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article53 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article53-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The duties of a member shall end in the event of the expiry of the term of office, resignation or compulsory retirement, in accordance with the law of the Member State concerned."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article53,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article53(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article53 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article53-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "A member shall be dismissed only in cases of serious misconduct or if the member no longer fulfils the conditions required for the performance of the duties."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article53,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article53(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article53 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article54-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the establishment of each supervisory authority;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article54,
        gdpr:article54-1,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article54(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article54 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfPoint gdpr:article54-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article54-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the qualifications and eligibility conditions required to be appointed as member of each supervisory authority;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article54,
        gdpr:article54-1,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article54(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article54 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfPoint gdpr:article54-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article54-1-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the rules and procedures for the appointment of the member or members of each supervisory authority;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article54,
        gdpr:article54-1,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article54(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article54 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfPoint gdpr:article54-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article54-1-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the duration of the term of the member or members of each supervisory authority of no less than four years, except for the first appointment after 24 May 2016, part of which may take place for a shorter period where that is necessary to protect the independence of the supervisory authority by means of a staggered appointment procedure;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article54,
        gdpr:article54-1,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article54(1)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article54 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfPoint gdpr:article54-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article54-1-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "whether and, if so, for how many terms the member or members of each supervisory authority is eligible for reappointment;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article54,
        gdpr:article54-1,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article54(1)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article54 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfPoint gdpr:article54-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article54-1-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the conditions governing the obligations of the member or members and staff of each supervisory authority, prohibitions on actions, occupations and benefits incompatible therewith during and after the term of office and rules governing the cessation of employment."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article54,
        gdpr:article54-1,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article54(1)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article54 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfPoint gdpr:article54-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article54-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The member or members and the staff of each supervisory authority shall, in accordance with Union or Member State law, be subject to a duty of professional secrecy both during and after their term of office, with regard to any confidential information which has come to their knowledge in the course of the performance of their tasks or exercise of their powers. During their term of office, that duty of professional secrecy shall in particular apply to reporting by natural persons of infringements of this Regulation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article54,
        gdpr:chapterVI,
        gdpr:chapterVI-1 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article54(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article54 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-1 .

gdpr:article55-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each supervisory authority shall be competent for the performance of the tasks assigned to and the exercise of the powers conferred on it in accordance with this Regulation on the territory of its own Member State."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article55,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article55(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article55 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article55-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where processing is carried out by public authorities or private bodies acting on the basis of point (c) or (e) of Article 6(1), the supervisory authority of the Member State concerned shall be competent. In such cases Article 56 does not apply."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article55,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article55(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article55 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article55-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Supervisory authorities shall not be competent to supervise processing operations of courts acting in their judicial capacity."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article55,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article55(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article55 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article56-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Without prejudice to Article 55, the supervisory authority of the main establishment or of the single establishment of the controller or processor shall be competent to act as lead supervisory authority for the cross-border processing carried out by that controller or processor in accordance with the procedure provided in Article 60."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article56,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article56(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article56 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article56-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "By derogation from paragraph 1, each supervisory authority shall be competent to handle a complaint lodged with it or a possible infringement of this Regulation, if the subject matter relates only to an establishment in its Member State or substantially affects data subjects only in its Member State."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article56,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article56(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article56 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article56-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "In the cases referred to in paragraph 2 of this Article, the supervisory authority shall inform the lead supervisory authority without delay on that matter. Within a period of three weeks after being informed the lead supervisory authority shall decide whether or not it will handle the case in accordance with the procedure provided in Article 60, taking into account whether or not there is an establishment of the controller or processor in the Member State of which the supervisory authority informed it."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article56,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article56(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article56 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article56-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where the lead supervisory authority decides to handle the case, the procedure provided in Article 60 shall apply. The supervisory authority which informed the lead supervisory authority may submit to the lead supervisory authority a draft for a decision. The lead supervisory authority shall take utmost account of that draft when preparing the draft decision referred to in Article 60(3)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article56,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article56(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article56 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article56-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where the lead supervisory authority decides not to handle the case, the supervisory authority which informed the lead supervisory authority shall handle it according to Articles 61 and 62."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article56,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article56(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article56 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article56-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The lead supervisory authority shall be the sole interlocutor of the controller or processor for the cross-border processing carried out by that controller or processor."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article56,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article56(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article56 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article57-1-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "monitor and enforce the application of this Regulation;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article57,
        gdpr:article57-1,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article57(1)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article57 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfPoint gdpr:article57-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article57-1-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "promote public awareness and understanding of the risks, rules, safeguards and rights in relation to processing. Activities addressed specifically to children shall receive specific attention;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article57,
        gdpr:article57-1,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article57(1)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article57 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfPoint gdpr:article57-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article57-1-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "advise, in accordance with Member State law, the national parliament, the government, and other institutions and bodies on legislative and administrative measures relating to the protection of natural persons' rights and freedoms with regard to processing;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article57,
        gdpr:article57-1,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article57(1)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article57 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfPoint gdpr:article57-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article57-1-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "promote the awareness of controllers and processors of their obligations under this Regulation;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article57,
        gdpr:article57-1,
        gdpr:chapterVI,
        gdpr:chapterVI-2 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article57(1)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article57 ;
    GDPRtext:isPartOfChapter gdpr:chapterVI ;
    GDPRtext:isPartOfPoint gdpr:article57-1 ;
    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

