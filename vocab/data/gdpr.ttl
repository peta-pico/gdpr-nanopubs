    GDPRtext:isPartOfSection gdpr:chapterVI-2 .

gdpr:article58-3-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "to authorise processing referred to in Article 36(5), if the law of the Member State requires such prior authorisation;"^^xsd:string ;
