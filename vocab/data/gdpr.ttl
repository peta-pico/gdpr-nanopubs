        GDPRtext:Point ;
    eli:description "The Board shall have a secretariat, which shall be provided by the European Data Protection Supervisor."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article75,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article75(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article75 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article75-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The secretariat shall perform its tasks exclusively under the instructions of the Chair of the Board."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article75,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article75(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article75 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article75-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The staff of the European Data Protection Supervisor involved in carrying out the tasks conferred on the Board by this Regulation shall be subject to separate reporting lines from the staff involved in carrying out tasks conferred on the European Data Protection Supervisor."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article75,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article75(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article75 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article75-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where appropriate, the Board and the European Data Protection Supervisor shall establish and publish a Memorandum of Understanding implementing this Article, determining the terms of their cooperation, and applicable to the staff of the European Data Protection Supervisor involved in carrying out the tasks conferred on the Board by this Regulation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article75,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article75(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article75 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article75-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The secretariat shall provide analytical, administrative and logistical support to the Board."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article75,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article75(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article75 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article75-6-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the day-to-day business of the Board;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article75,
        gdpr:article75-6,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article75(6)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article75 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article75-6 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article75-6-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "communication between the members of the Board, its Chair and the Commission;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article75,
        gdpr:article75-6,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article75(6)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article75 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article75-6 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article75-6-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "communication with other institutions and the public;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article75,
        gdpr:article75-6,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article75(6)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article75 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article75-6 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article75-6-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the use of electronic means for the internal and external communication;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article75,
        gdpr:article75-6,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article75(6)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article75 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article75-6 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article75-6-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the translation of relevant information;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article75,
        gdpr:article75-6,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article75(6)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article75 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article75-6 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article75-6-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the preparation and follow-up of the meetings of the Board;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article75,
        gdpr:article75-6,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article75(6)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article75 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article75-6 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article75-6-g a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the preparation, drafting and publication of opinions, decisions on the settlement of disputes between supervisory authorities and other texts adopted by the Board."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article75,
        gdpr:article75-6,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "g"^^xsd:string ;
    eli:title_alternative "Article75(6)(g)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article75 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfPoint gdpr:article75-6 ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article76-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The discussions of the Board shall be confidential where the Board deems it necessary, as provided for in its rules of procedure."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article76,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article76(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article76 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article76-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:cites gdpr:citation21 ;
    eli:description "Access to documents submitted to members of the Board, experts and representatives of third parties shall be governed by Regulation (EC) No 1049/2001 of the European Parliament and of the Council (21)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article76,
        gdpr:chapterVII,
        gdpr:chapterVII-3 ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article76(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article76 ;
    GDPRtext:isPartOfChapter gdpr:chapterVII ;
    GDPRtext:isPartOfSection gdpr:chapterVII-3 .

gdpr:article77-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Without prejudice to any other administrative or judicial remedy, every data subject shall have the right to lodge a complaint with a supervisory authority, in particular in the Member State of his or her habitual residence, place of work or place of the alleged infringement if the data subject considers that the processing of personal data relating to him or her infringes this Regulation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article77,
        gdpr:chapterVIII ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article77(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article77 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article77-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The supervisory authority with which the complaint has been lodged shall inform the complainant on the progress and the outcome of the complaint including the possibility of a judicial remedy pursuant to Article 78."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article77,
        gdpr:chapterVIII ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article77(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article77 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article78-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Without prejudice to any other administrative or non-judicial remedy, each natural or legal person shall have the right to an effective judicial remedy against a legally binding decision of a supervisory authority concerning them."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article78,
        gdpr:chapterVIII ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article78(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article78 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article78-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Without prejudice to any other administrative or non-judicial remedy, each data subject shall have the right to a an effective judicial remedy where the supervisory authority which is competent pursuant to Articles 55 and 56 does not handle a complaint or does not inform the data subject within three months on the progress or outcome of the complaint lodged pursuant to Article 77."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article78,
        gdpr:chapterVIII ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article78(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article78 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article78-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Proceedings against a supervisory authority shall be brought before the courts of the Member State where the supervisory authority is established."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article78,
        gdpr:chapterVIII ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article78(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article78 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article78-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where proceedings are brought against a decision of a supervisory authority which was preceded by an opinion or a decision of the Board in the consistency mechanism, the supervisory authority shall forward that opinion or decision to the court."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article78,
        gdpr:chapterVIII ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article78(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article78 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article79-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Without prejudice to any available administrative or non-judicial remedy, including the right to lodge a complaint with a supervisory authority pursuant to Article 77, each data subject shall have the right to an effective judicial remedy where he or she considers that his or her rights under this Regulation have been infringed as a result of the processing of his or her personal data in non-compliance with this Regulation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article79,
        gdpr:chapterVIII ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article79(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article79 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article79-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Proceedings against a controller or a processor shall be brought before the courts of the Member State where the controller or processor has an establishment. Alternatively, such proceedings may be brought before the courts of the Member State where the data subject has his or her habitual residence, unless the controller or processor is a public authority of a Member State acting in the exercise of its public powers."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article79,
        gdpr:chapterVIII ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article79(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article79 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article8-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Member States may provide by law for a lower age for those purposes provided that such lower age is not below 13 years."^^xsd:string,
        "Where point (a) of Article 6(1) applies, in relation to the offer of information society services directly to a child, the processing of the personal data of a child shall be lawful where the child is at least 16 years old. Where the child is below the age of 16 years, such processing shall be lawful only if and to the extent that consent is given or authorised by the holder of parental responsibility over the child."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article8,
        gdpr:chapterII ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article8(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article8 ;
    GDPRtext:isPartOfChapter gdpr:chapterII .

gdpr:article8-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The controller shall make reasonable efforts to verify in such cases that consent is given or authorised by the holder of parental responsibility over the child, taking into consideration available technology."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article8,
        gdpr:chapterII ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article8(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article8 ;
    GDPRtext:isPartOfChapter gdpr:chapterII .

gdpr:article8-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Paragraph 1 shall not affect the general contract law of Member States such as the rules on the validity, formation or effect of a contract in relation to a child."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article8,
        gdpr:chapterII ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article8(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article8 ;
    GDPRtext:isPartOfChapter gdpr:chapterII .

gdpr:article80-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The data subject shall have the right to mandate a not-for-profit body, organisation or association which has been properly constituted in accordance with the law of a Member State, has statutory objectives which are in the public interest, and is active in the field of the protection of data subjects' rights and freedoms with regard to the protection of their personal data to lodge the complaint on his or her behalf, to exercise the rights referred to in Articles 77, 78 and 79 on his or her behalf, and to exercise the right to receive compensation referred to in Article 82 on his or her behalf where provided for by Member State law."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article80,
        gdpr:chapterVIII ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article80(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article80 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article80-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Member States may provide that any body, organisation or association referred to in paragraph 1 of this Article, independently of a data subject's mandate, has the right to lodge, in that Member State, a complaint with the supervisory authority which is competent pursuant to Article 77 and to exercise the rights referred to in Articles 78 and 79 if it considers that the rights of a data subject under this Regulation have been infringed as a result of the processing."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article80,
        gdpr:chapterVIII ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article80(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article80 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article81-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where a competent court of a Member State has information on proceedings, concerning the same subject matter as regards processing by the same controller or processor, that are pending in a court in another Member State, it shall contact that court in the other Member State to confirm the existence of such proceedings."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article81,
        gdpr:chapterVIII ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article81(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article81 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article81-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where proceedings concerning the same subject matter as regards processing of the same controller or processor are pending in a court in another Member State, any competent court other than the court first seized may suspend its proceedings."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article81,
        gdpr:chapterVIII ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article81(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article81 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article81-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where those proceedings are pending at first instance, any court other than the court first seized may also, on the application of one of the parties, decline jurisdiction if the court first seized has jurisdiction over the actions in question and its law permits the consolidation thereof."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article81,
        gdpr:chapterVIII ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article81(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article81 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article82-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Any person who has suffered material or non-material damage as a result of an infringement of this Regulation shall have the right to receive compensation from the controller or processor for the damage suffered."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article82,
        gdpr:chapterVIII ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article82(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article82 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article82-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Any controller involved in processing shall be liable for the damage caused by processing which infringes this Regulation. A processor shall be liable for the damage caused by processing only where it has not complied with obligations of this Regulation specifically directed to processors or where it has acted outside or contrary to lawful instructions of the controller."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article82,
        gdpr:chapterVIII ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article82(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article82 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article82-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "A controller or processor shall be exempt from liability under paragraph 2 if it proves that it is not in any way responsible for the event giving rise to the damage."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article82,
        gdpr:chapterVIII ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article82(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article82 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article82-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where more than one controller or processor, or both a controller and a processor, are involved in the same processing and where they are, under paragraphs 2 and 3, responsible for any damage caused by processing, each controller or processor shall be held liable for the entire damage in order to ensure effective compensation of the data subject."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article82,
        gdpr:chapterVIII ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article82(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article82 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article82-5 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where a controller or processor has, in accordance with paragraph 4, paid full compensation for the damage suffered, that controller or processor shall be entitled to claim back from the other controllers or processors involved in the same processing that part of the compensation corresponding to their part of responsibility for the damage, in accordance with the conditions set out in paragraph 2."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article82,
        gdpr:chapterVIII ;
    eli:number "5"^^xsd:string ;
    eli:title_alternative "Article82(5)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article82 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article82-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Court proceedings for exercising the right to receive compensation shall be brought before the courts competent under the law of the Member State referred to in Article 79(2)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article82,
        gdpr:chapterVIII ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article82(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article82 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article83-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each supervisory authority shall ensure that the imposition of administrative fines pursuant to this Article in respect of infringements of this Regulation referred to in paragraphs 4, 5 and 6 shall in each individual case be effective, proportionate and dissuasive."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:chapterVIII ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article83(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article83-2-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the nature, gravity and duration of the infringement taking into account the nature scope or purpose of the processing concerned as well as the number of data subjects affected and the level of damage suffered by them;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-2,
        gdpr:chapterVIII ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article83(2)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-2 .

gdpr:article83-2-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the intentional or negligent character of the infringement;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-2,
        gdpr:chapterVIII ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article83(2)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-2 .

gdpr:article83-2-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "any action taken by the controller or processor to mitigate the damage suffered by data subjects;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-2,
        gdpr:chapterVIII ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article83(2)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-2 .

gdpr:article83-2-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the degree of responsibility of the controller or processor taking into account technical and organisational measures implemented by them pursuant to Articles 25 and 32;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-2,
        gdpr:chapterVIII ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article83(2)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-2 .

gdpr:article83-2-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "any relevant previous infringements by the controller or processor;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-2,
        gdpr:chapterVIII ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article83(2)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-2 .

gdpr:article83-2-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the degree of cooperation with the supervisory authority, in order to remedy the infringement and mitigate the possible adverse effects of the infringement;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-2,
        gdpr:chapterVIII ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article83(2)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-2 .

gdpr:article83-2-g a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the categories of personal data affected by the infringement;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-2,
        gdpr:chapterVIII ;
    eli:number "g"^^xsd:string ;
    eli:title_alternative "Article83(2)(g)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-2 .

gdpr:article83-2-h a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the manner in which the infringement became known to the supervisory authority, in particular whether, and if so to what extent, the controller or processor notified the infringement;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-2,
        gdpr:chapterVIII ;
    eli:number "h"^^xsd:string ;
    eli:title_alternative "Article83(2)(h)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-2 .

gdpr:article83-2-i a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "where measures referred to in Article 58(2) have previously been ordered against the controller or processor concerned with regard to the same subject-matter, compliance with those measures;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-2,
        gdpr:chapterVIII ;
    eli:number "i"^^xsd:string ;
    eli:title_alternative "Article83(2)(i)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-2 .

gdpr:article83-2-j a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "adherence to approved codes of conduct pursuant to Article 40 or approved certification mechanisms pursuant to Article 42; and"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-2,
        gdpr:chapterVIII ;
    eli:number "j"^^xsd:string ;
    eli:title_alternative "Article83(2)(j)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-2 .

gdpr:article83-2-k a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "any other aggravating or mitigating factor applicable to the circumstances of the case, such as financial benefits gained, or losses avoided, directly or indirectly, from the infringement."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-2,
        gdpr:chapterVIII ;
    eli:number "k"^^xsd:string ;
    eli:title_alternative "Article83(2)(k)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-2 .

gdpr:article83-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "If a controller or processor intentionally or negligently, for the same or linked processing operations, infringes several provisions of this Regulation, the total amount of the administrative fine shall not exceed the amount specified for the gravest infringement."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:chapterVIII ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article83(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article83-4-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the obligations of the controller and the processor pursuant to Articles 8, 11, 25 to 39 and 42 and 43;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-4,
        gdpr:chapterVIII ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article83(4)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-4 .

gdpr:article83-4-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the obligations of the certification body pursuant to Articles 42 and 43;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-4,
        gdpr:chapterVIII ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article83(4)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-4 .

gdpr:article83-4-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the obligations of the monitoring body pursuant to Article 41(4)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-4,
        gdpr:chapterVIII ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article83(4)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-4 .

gdpr:article83-5-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the basic principles for processing, including conditions for consent, pursuant to Articles 5, 6, 7 and 9;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-5,
        gdpr:chapterVIII ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article83(5)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-5 .

gdpr:article83-5-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the data subjects' rights pursuant to Articles 12 to 22;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-5,
        gdpr:chapterVIII ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article83(5)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-5 .

gdpr:article83-5-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the transfers of personal data to a recipient in a third country or an international organisation pursuant to Articles 44 to 49;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-5,
        gdpr:chapterVIII ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article83(5)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-5 .

gdpr:article83-5-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "any obligations pursuant to Member State law adopted under Chapter IX;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-5,
        gdpr:chapterVIII ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article83(5)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-5 .

gdpr:article83-5-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "non-compliance with an order or a temporary or definitive limitation on processing or the suspension of data flows by the supervisory authority pursuant to Article 58(2) or failure to provide access in violation of Article 58(1)."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:article83-5,
        gdpr:chapterVIII ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article83(5)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII ;
    GDPRtext:isPartOfPoint gdpr:article83-5 .

gdpr:article83-6 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Non-compliance with an order by the supervisory authority as referred to in Article 58(2) shall, in accordance with paragraph 2 of this Article, be subject to administrative fines up to 20 000 000 EUR, or in the case of an undertaking, up to 4 % of the total worldwide annual turnover of the preceding financial year, whichever is higher."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:chapterVIII ;
    eli:number "6"^^xsd:string ;
    eli:title_alternative "Article83(6)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article83-7 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Without prejudice to the corrective powers of supervisory authorities pursuant to Article 58(2), each Member State may lay down the rules on whether and to what extent administrative fines may be imposed on public authorities and bodies established in that Member State."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:chapterVIII ;
    eli:number "7"^^xsd:string ;
    eli:title_alternative "Article83(7)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article83-8 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The exercise by the supervisory authority of its powers under this Article shall be subject to appropriate procedural safeguards in accordance with Union and Member State law, including effective judicial remedy and due process."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:chapterVIII ;
    eli:number "8"^^xsd:string ;
    eli:title_alternative "Article83(8)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article83-9 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where the legal system of the Member State does not provide for administrative fines, this Article may be applied in such a manner that the fine is initiated by the competent supervisory authority and imposed by competent national courts, while ensuring that those legal remedies are effective and have an equivalent effect to the administrative fines imposed by supervisory authorities. In any event, the fines imposed shall be effective, proportionate and dissuasive. Those Member States shall notify to the Commission the provisions of their laws which they adopt pursuant to this paragraph by 25 May 2018 and, without delay, any subsequent amendment law or amendment affecting them."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article83,
        gdpr:chapterVIII ;
    eli:number "9"^^xsd:string ;
    eli:title_alternative "Article83(9)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article83 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article84-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Member States shall lay down the rules on other penalties applicable to infringements of this Regulation in particular for infringements which are not subject to administrative fines pursuant to Article 83, and shall take all measures necessary to ensure that they are implemented. Such penalties shall be effective, proportionate and dissuasive."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article84,
        gdpr:chapterVIII ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article84(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article84 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article84-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each Member State shall notify to the Commission the provisions of its law which it adopts pursuant to paragraph 1, by 25 May 2018 and, without delay, any subsequent amendment affecting them."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article84,
        gdpr:chapterVIII ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article84(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article84 ;
    GDPRtext:isPartOfChapter gdpr:chapterVIII .

gdpr:article85-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Member States shall by law reconcile the right to the protection of personal data pursuant to this Regulation with the right to freedom of expression and information, including processing for journalistic purposes and the purposes of academic, artistic or literary expression."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article85,
        gdpr:chapterIX ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article85(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article85 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article85-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "For processing carried out for journalistic purposes or the purpose of academic artistic or literary expression, Member States shall provide for exemptions or derogations from Chapter II (principles), Chapter III (rights of the data subject), Chapter IV (controller and processor), Chapter V (transfer of personal data to third countries or international organisations), Chapter VI (independent supervisory authorities), Chapter VII (cooperation and consistency) and Chapter IX (specific data processing situations) if they are necessary to reconcile the right to the protection of personal data with the freedom of expression and information."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article85,
        gdpr:chapterIX ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article85(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article85 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article85-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each Member State shall notify to the Commission the provisions of its law which it has adopted pursuant to paragraph 2 and, without delay, any subsequent amendment law or amendment affecting them."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article85,
        gdpr:chapterIX ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article85(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article85 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article86-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Personal data in official documents held by a public authority or a public body or a private body for the performance of a task carried out in the public interest may be disclosed by the authority or body in accordance with Union or Member State law to which the public authority or body is subject in order to reconcile public access to official documents with the right to the protection of personal data pursuant to this Regulation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article86,
        gdpr:chapterIX ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article86(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article86 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article87-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Member States may further determine the specific conditions for the processing of a national identification number or any other identifier of general application. In that case the national identification number or any other identifier of general application shall be used only under appropriate safeguards for the rights and freedoms of the data subject pursuant to this Regulation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article87,
        gdpr:chapterIX ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article87(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article87 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article88-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Member States may, by law or by collective agreements, provide for more specific rules to ensure the protection of the rights and freedoms in respect of the processing of employees' personal data in the employment context, in particular for the purposes of the recruitment, the performance of the contract of employment, including discharge of obligations laid down by law or by collective agreements, management, planning and organisation of work, equality and diversity in the workplace, health and safety at work, protection of employer's or customer's property and for the purposes of the exercise and enjoyment, on an individual or collective basis, of rights and benefits related to employment, and for the purpose of the termination of the employment relationship."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article88,
        gdpr:chapterIX ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article88(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article88 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article88-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Those rules shall include suitable and specific measures to safeguard the data subject's human dignity, legitimate interests and fundamental rights, with particular regard to the transparency of processing, the transfer of personal data within a group of undertakings, or a group of enterprises engaged in a joint economic activity and monitoring systems at the work place."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article88,
        gdpr:chapterIX ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article88(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article88 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article88-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each Member State shall notify to the Commission those provisions of its law which it adopts pursuant to paragraph 1, by 25 May 2018 and, without delay, any subsequent amendment affecting them."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article88,
        gdpr:chapterIX ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article88(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article88 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article89-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Processing for archiving purposes in the public interest, scientific or historical research purposes or statistical purposes, shall be subject to appropriate safeguards, in accordance with this Regulation, for the rights and freedoms of the data subject. Those safeguards shall ensure that technical and organisational measures are in place in particular in order to ensure respect for the principle of data minimisation. Those measures may include pseudonymisation provided that those purposes can be fulfilled in that manner. Where those purposes can be fulfilled by further processing which does not permit or no longer permits the identification of data subjects, those purposes shall be fulfilled in that manner."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article89,
        gdpr:chapterIX ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article89(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article89 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article89-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where personal data are processed for scientific or historical research purposes or statistical purposes, Union or Member State law may provide for derogations from the rights referred to in Articles 15, 16, 18 and 21 subject to the conditions and safeguards referred to in paragraph 1 of this Article in so far as such rights are likely to render impossible or seriously impair the achievement of the specific purposes, and such derogations are necessary for the fulfilment of those purposes."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article89,
        gdpr:chapterIX ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article89(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article89 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article89-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where personal data are processed for archiving purposes in the public interest, Union or Member State law may provide for derogations from the rights referred to in Articles 15, 16, 18, 19, 20 and 21 subject to the conditions and safeguards referred to in paragraph 1 of this Article in so far as such rights are likely to render impossible or seriously impair the achievement of the specific purposes, and such derogations are necessary for the fulfilment of those purposes."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article89,
        gdpr:chapterIX ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article89(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article89 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article89-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where processing referred to in paragraphs 2 and 3 serves at the same time another purpose, the derogations shall apply only to processing for the purposes referred to in those paragraphs."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article89,
        gdpr:chapterIX ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article89(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article89 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article9-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Processing of personal data revealing racial or ethnic origin, political opinions, religious or philosophical beliefs, or trade union membership, and the processing of genetic data, biometric data for the purpose of uniquely identifying a natural person, data concerning health or data concerning a natural person's sex life or sexual orientation shall be prohibited."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article9,
        gdpr:chapterII ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article9(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article9 ;
    GDPRtext:isPartOfChapter gdpr:chapterII .

gdpr:article9-2-a a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "the data subject has given explicit consent to the processing of those personal data for one or more specified purposes, except where Union or Member State law provide that the prohibition referred to in paragraph 1 may not be lifted by the data subject;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article9,
        gdpr:article9-2,
        gdpr:chapterII ;
    eli:number "a"^^xsd:string ;
    eli:title_alternative "Article9(2)(a)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article9 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article9-2 .

gdpr:article9-2-b a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing is necessary for the purposes of carrying out the obligations and exercising specific rights of the controller or of the data subject in the field of employment and social security and social protection law in so far as it is authorised by Union or Member State law or a collective agreement pursuant to Member State law providing for appropriate safeguards for the fundamental rights and the interests of the data subject;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article9,
        gdpr:article9-2,
        gdpr:chapterII ;
    eli:number "b"^^xsd:string ;
    eli:title_alternative "Article9(2)(b)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article9 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article9-2 .

gdpr:article9-2-c a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing is necessary to protect the vital interests of the data subject or of another natural person where the data subject is physically or legally incapable of giving consent;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article9,
        gdpr:article9-2,
        gdpr:chapterII ;
    eli:number "c"^^xsd:string ;
    eli:title_alternative "Article9(2)(c)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article9 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article9-2 .

gdpr:article9-2-d a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing is carried out in the course of its legitimate activities with appropriate safeguards by a foundation, association or any other not-for-profit body with a political, philosophical, religious or trade union aim and on condition that the processing relates solely to the members or to former members of the body or to persons who have regular contact with it in connection with its purposes and that the personal data are not disclosed outside that body without the consent of the data subjects;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article9,
        gdpr:article9-2,
        gdpr:chapterII ;
    eli:number "d"^^xsd:string ;
    eli:title_alternative "Article9(2)(d)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article9 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article9-2 .

gdpr:article9-2-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing relates to personal data which are manifestly made public by the data subject;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article9,
        gdpr:article9-2,
        gdpr:chapterII ;
    eli:number "e"^^xsd:string ;
    eli:title_alternative "Article9(2)(e)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article9 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article9-2 .

gdpr:article9-2-f a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing is necessary for the establishment, exercise or defence of legal claims or whenever courts are acting in their judicial capacity;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article9,
        gdpr:article9-2,
        gdpr:chapterII ;
    eli:number "f"^^xsd:string ;
    eli:title_alternative "Article9(2)(f)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article9 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article9-2 .

gdpr:article9-2-g a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing is necessary for reasons of substantial public interest, on the basis of Union or Member State law which shall be proportionate to the aim pursued, respect the essence of the right to data protection and provide for suitable and specific measures to safeguard the fundamental rights and the interests of the data subject;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article9,
        gdpr:article9-2,
        gdpr:chapterII ;
    eli:number "g"^^xsd:string ;
    eli:title_alternative "Article9(2)(g)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article9 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article9-2 .

gdpr:article9-2-h a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing is necessary for the purposes of preventive or occupational medicine, for the assessment of the working capacity of the employee, medical diagnosis, the provision of health or social care or treatment or the management of health or social care systems and services on the basis of Union or Member State law or pursuant to contract with a health professional and subject to the conditions and safeguards referred to in paragraph 3;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article9,
        gdpr:article9-2,
        gdpr:chapterII ;
    eli:number "h"^^xsd:string ;
    eli:title_alternative "Article9(2)(h)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article9 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article9-2 .

gdpr:article9-2-i a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing is necessary for reasons of public interest in the area of public health, such as protecting against serious cross-border threats to health or ensuring high standards of quality and safety of health care and of medicinal products or medical devices, on the basis of Union or Member State law which provides for suitable and specific measures to safeguard the rights and freedoms of the data subject, in particular professional secrecy;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article9,
        gdpr:article9-2,
        gdpr:chapterII ;
    eli:number "i"^^xsd:string ;
    eli:title_alternative "Article9(2)(i)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article9 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article9-2 .

gdpr:article9-2-j a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "processing is necessary for archiving purposes in the public interest, scientific or historical research purposes or statistical purposes in accordance with Article 89(1) based on Union or Member State law which shall be proportionate to the aim pursued, respect the essence of the right to data protection and provide for suitable and specific measures to safeguard the fundamental rights and the interests of the data subject."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article9,
        gdpr:article9-2,
        gdpr:chapterII ;
    eli:number "j"^^xsd:string ;
    eli:title_alternative "Article9(2)(j)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article9 ;
    GDPRtext:isPartOfChapter gdpr:chapterII ;
    GDPRtext:isPartOfPoint gdpr:article9-2 .

gdpr:article9-3 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Personal data referred to in paragraph 1 may be processed for the purposes referred to in point (h) of paragraph 2 when those data are processed by or under the responsibility of a professional subject to the obligation of professional secrecy under Union or Member State law or rules established by national competent bodies or by another person also subject to an obligation of secrecy under Union or Member State law or rules established by national competent bodies."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article9,
        gdpr:chapterII ;
    eli:number "3"^^xsd:string ;
    eli:title_alternative "Article9(3)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article9 ;
    GDPRtext:isPartOfChapter gdpr:chapterII .

gdpr:article9-4 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Member States may maintain or introduce further conditions, including limitations, with regard to the processing of genetic data, biometric data or data concerning health."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article9,
        gdpr:chapterII ;
    eli:number "4"^^xsd:string ;
    eli:title_alternative "Article9(4)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article9 ;
    GDPRtext:isPartOfChapter gdpr:chapterII .

gdpr:article90-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Member States may adopt specific rules to set out the powers of the supervisory authorities laid down in points (e) and (f) of Article 58(1) in relation to controllers or processors that are subject, under Union or Member State law or rules established by national competent bodies, to an obligation of professional secrecy or other equivalent obligations of secrecy where this is necessary and proportionate to reconcile the right of the protection of personal data with the obligation of secrecy. Those rules shall apply only with regard to personal data which the controller or processor has received as a result of or has obtained in an activity covered by that obligation of secrecy."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article90,
        gdpr:chapterIX ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article90(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article90 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article90-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Each Member State shall notify to the Commission the rules adopted pursuant to paragraph 1, by 25 May 2018 and, without delay, any subsequent amendment affecting them."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article90,
        gdpr:chapterIX ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article90(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article90 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article91-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Where in a Member State, churches and religious associations or communities apply, at the time of entry into force of this Regulation, comprehensive rules relating to the protection of natural persons with regard to processing, such rules may continue to apply, provided that they are brought into line with this Regulation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article91,
        gdpr:chapterIX ;
    eli:number "1"^^xsd:string ;
    eli:title_alternative "Article91(1)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article91 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article91-2 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "Churches and religious associations which apply comprehensive rules in accordance with paragraph 1 of this Article shall be subject to the supervision of an independent supervisory authority, which may be specific, provided that it fulfils the conditions laid down in Chapter VI of this Regulation."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article91,
        gdpr:chapterIX ;
    eli:number "2"^^xsd:string ;
    eli:title_alternative "Article91(2)"^^xsd:string ;
    GDPRtext:isPartOfArticle gdpr:article91 ;
    GDPRtext:isPartOfChapter gdpr:chapterIX .

gdpr:article92-1 a eli:LegalResourceSubdivision,
        GDPRtext:Point ;
    eli:description "The power to adopt delegated acts is conferred on the Commission subject to the conditions laid down in this Article."^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article92,
