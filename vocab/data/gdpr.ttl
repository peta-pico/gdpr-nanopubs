    GDPRtext:isPartOfSection gdpr:chapterIII-2 .

gdpr:article13-2-e a eli:LegalResourceSubdivision,
        GDPRtext:SubPoint ;
    eli:description "whether the provision of personal data is a statutory or contractual requirement, or a requirement necessary to enter into a contract, as well as whether the data subject is obliged to provide the personal data and of the possible consequences of failure to provide such data;"^^xsd:string ;
    eli:is_part_of gdpr:GDPR,
        gdpr:article13,
        gdpr:article13-2,
        gdpr:chapterIII,
        gdpr:chapterIII-2 ;
    eli:number "e"^^xsd:string ;
